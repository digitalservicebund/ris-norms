            <akn:num GUID="25a9acae-7463-4490-bc3f-8258b629d7e9" eId="art-1_bezeichnung-1">
               <akn:marker name="1"/>Artikel 1</akn:num>
            <akn:heading GUID="92827aa8-8118-4207-9f93-589345f0bab6" eId="art-1_überschrift-1">Änderung des Vereinsgesetzes</akn:heading>
            <!-- Absatz (1) -->
            <akn:paragraph GUID="48ead358-f901-41d3-a135-e372d5ef97b1" eId="art-1_abs-1">
               <akn:num GUID="ef3a32d2-df20-4978-914b-cd6288872208"
                        eId="art-1_abs-1_bezeichnung-1">
                  <akn:marker name="1"/>
               </akn:num>
               <akn:list GUID="41675622-ed62-46e3-869f-94d99908b010" eId="art-1_abs-1_untergl-1">
                  <akn:intro GUID="5d6fc824-7926-43b4-b243-b0096da183f9"
                             eId="art-1_abs-1_untergl-1_intro-1">
                     <akn:p GUID="04879ca1-994b-4eb2-b59b-032e528d9ce5"
                            eId="art-1_abs-1_untergl-1_intro-1_text-1"> Das <akn:affectedDocument href="eli/bund/bgbl-1/1964/s593/1964-08-05/1/deu/regelungstext-1">Vereinsgesetz vom 5. August 1964 (BGBl. I S. 593), das zuletzt
                           durch … geändert worden ist</akn:affectedDocument>, wird wie folgt geändert:</akn:p>
                  </akn:intro>
                  <!-- Nummer 1 -->
                  <akn:point GUID="b5fa1383-f26a-4904-a638-f48711fbcf2d"
                             eId="art-1_abs-1_untergl-1_listenelem-1">
                     <akn:num GUID="6f0f92b3-1a51-440c-9137-b44ab9d990ac"
                              eId="art-1_abs-1_untergl-1_listenelem-1_bezeichnung-1">
                        <akn:marker name="2"/>2.</akn:num>
                     <akn:content GUID="6cb14ab5-3a7f-45f4-9e85-00ac2fb0fe5e"
                                  eId="art-1_abs-1_untergl-1_listenelem-1_inhalt-1">
                        <akn:p GUID="db3fbe0f-b758-4cc4-b528-a723cacad94a"
                               eId="art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1">
                           <akn:mod GUID="148c2f06-6e33-4af8-9f4a-3da67c888510"
                                    eId="art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1"
                                    refersTo="aenderungsbefehl-ersetzen">In <akn:ref href="eli/bund/bgbl-1/1964/s593/1964-08-05/1/deu/regelungstext-1/para-20_abs-1_untergl-1_listenelem-2_inhalt-1_text-1/9-34.xml">§ 20 Absatz 1 Satz 2</akn:ref> wird
                              die Angabe <akn:quotedText startQuote="„" endQuote="“">§ 9 Abs. 1 Satz</akn:quotedText> durch die Wörter <akn:quotedText startQuote="„" endQuote="“">§ 9 Absatz 1 Satz</akn:quotedText> ersetzt.</akn:mod>
                        </akn:p>
                     </akn:content>
                  </akn:point>
               </akn:list>
            </akn:paragraph>
         </akn:article>
<!-- Artikel 2: Geltungszeitregel-->
         <akn:article period="#geltungszeitgr-1"
                      GUID="aaae12b5-0c74-4e51-a286-d6051ff5d21b"
                      eId="art-2"
                      refersTo="geltungszeitregel">
            <akn:num GUID="1bc12642-f00c-4b55-8388-5e8870e6e706" eId="art-2_bezeichnung-1">
               <akn:marker name="2"/>Artikel 2</akn:num>
            <akn:heading GUID="59a7dc28-e095-4da6-ba78-278a0d69a3fd" eId="art-2_überschrift-1">Inkrafttreten</akn:heading>
            <!-- Absatz (1) -->
            <akn:paragraph GUID="0b1590b0-8945-44a0-bf44-ebfb7d8c3bd8" eId="art-2_abs-1">
               <akn:num GUID="c46a1cbc-f823-4a18-9b0c-0f131244a58e"
                        eId="art-2_abs-1_bezeichnung-1">
                  <akn:marker name="1"/>
               </akn:num>
               <akn:content GUID="3e004e1f-f1bc-42a7-8042-2c1d77df81aa" eId="art-2_abs-1_inhalt-1">
                  <akn:p GUID="52406e40-b866-410c-b097-af69e6248f58"
                         eId="art-2_abs-1_inhalt-1_text-1"> Dieses Gesetz tritt <akn:date GUID="2ee89811-5368-46e0-acf8-a598506cc956"
                               eId="art-2_abs-1_inhalt-1_text-1_datum-1"
                               date="2017-03-16"
                               refersTo="inkrafttreten-datum">am Tag nach der Verkündung</akn:date> in Kraft. </akn:p>
               </akn:content>
            </akn:paragraph>
         </akn:article>
