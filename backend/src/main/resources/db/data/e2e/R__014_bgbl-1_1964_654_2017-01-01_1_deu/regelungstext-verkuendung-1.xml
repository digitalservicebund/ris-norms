      <akn:preamble GUID="6492fbac-1c97-40b1-b3fa-854d67cd4634" eId="präambel-n1">
         <akn:formula refersTo="eingangsformel"
                      name="attributsemantik-noch-undefiniert"
                      GUID="49c48cc6-55bc-4bce-91fe-d9a3e2a5b6ca"
                      eId="präambel-n1_formel-n1">
            <akn:p GUID="dfdf0e6a-a229-4349-8907-b4719db26e06"
                   eId="präambel-n1_formel-n1_text-n1">Der Bundestag hat mit Zustimmung des Bundesrates das folgende Gesetz beschlossen:</akn:p>
         </akn:formula>
      </akn:preamble>
      <akn:body GUID="826611d7-9e33-4ae0-aedb-335ff4740f33" eId="hauptteil-n1">
         <akn:article GUID="ca88e7f6-421b-4cbc-93eb-df12c6fa2150"
                      eId="art-z20"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1">
            <akn:num GUID="1505baab-fe80-4000-92e6-3501ec48f9f2"
                     eId="art-z20_bezeichnung-n1"> § 20</akn:num>
            <akn:paragraph GUID="21e61ed6-ea6b-46f4-96f7-0c736bc689cf" eId="art-z20_abs-z1">
               <akn:num GUID="39fb4003-ada7-4a61-8d6b-8e79e924eafa"
                        eId="art-z20_abs-z1_bezeichnung-n1"> (1) </akn:num>
               <akn:content GUID="7a7fe021-000c-4436-b472-2540f8f13a38"
                            eId="art-z20_abs-z1_inhalt-n1">
                  <akn:blockList GUID="43b08deb-d754-4c17-8227-448ba822bf1b"
                                 eId="art-z20_abs-z1_inhalt-n1_liste-n1">
                     <akn:listIntroduction GUID="b2e06f37-acbf-4bf9-a0d6-d0a936d87d5e"
                                           eId="art-z20_abs-z1_inhalt-n1_liste-n1_listeneing-n1">
                     Wer
                  </akn:listIntroduction>
                     <akn:item GUID="d7aee1ef-937a-4de1-82cf-574d34c39217"
                               eId="art-z20_abs-z1_inhalt-n1_liste-n1_listenelem-n1">
                        <akn:num GUID="865b4414-020e-417d-b378-37062d138c8b"
                                 eId="art-z20_abs-z1_inhalt-n1_liste-n1_listenelem-n1_bezeichnung-n1"> 1. </akn:num>
                        <akn:p GUID="c719c810-be00-419b-b2ff-0c736f2cae32"
                               eId="art-z20_abs-z1_inhalt-n1_liste-n1_listenelem-n1_text-n1">entgegen einem vollziehbaren Verbot den Verein fortführt, seinen organisatorischen Zusammenhalt auf andere Weise aufrechterhält, sich an ihm als Mitglied beteiligt, für ihn wirbt, ihn unterstützt oder eine Tätigkeit ausübt (§ 18 Satz 2) oder</akn:p>
                     </akn:item>
                     <akn:item GUID="50121d03-0ba8-4891-8994-d7b5816539e9"
                               eId="art-z20_abs-z1_inhalt-n1_liste-n1_listenelem-n2">
                        <akn:num GUID="ed7b2b30-c8ea-403d-b73c-bcead02cbafe"
                                 eId="art-z20_abs-z1_inhalt-n1_liste-n1_listenelem-n2_bezeichnung-n1"> 2. </akn:num>
                        <akn:p GUID="988259bb-29f6-4f91-88b4-0205b916119a"
                               eId="art-z20_abs-z1_inhalt-n1_liste-n1_listenelem-n2_text-n1">entgegen § 9 Abs. 1 Satz 2, Abs. 2 Kennzeichen eines verbotenen Vereins oder einer Ersatzorganisation verwendet,</akn:p>
                     </akn:item>
                     <akn:listWrapUp GUID="e9d94c8e-c415-4c79-8e20-02f54510d928"
                                     eId="art-z20_abs-z1_inhalt-n1_liste-n1_listenschl-n1">
                     wird mit Gefängnis bis zu einem Jahr oder mit Geldstrafe bestraft, wenn die Tat nicht in den §§ 49 b, 90 a, 90 b, 96 a, 128 oder 129 des Strafgesetzbuches, mit schwerer Strafe bedroht ist.
                  </akn:listWrapUp>
                  </akn:blockList>
               </akn:content>
            </akn:paragraph>
            <akn:paragraph GUID="25343921-f513-44ad-80ee-834d522e56ce" eId="art-z20_abs-z2">
               <akn:num GUID="7d3f990c-0063-48b0-a5a9-9338a66d3d70"
                        eId="art-z20_abs-z2_bezeichnung-n1"> (2) </akn:num>
               <akn:content GUID="305f79fc-4ca0-432b-9af8-c7739d538c3c"
                            eId="art-z20_abs-z2_inhalt-n1">
                  <akn:p GUID="8d01173f-cda0-4f1e-9810-78faf514fbe8"
                         eId="art-z20_abs-z2_inhalt-n1_text-n1">In den Fällen des Absatzes 1 Nr. 1 gilt § 90 a Abs. 5 und 6 des Strafgesetzbuches entsprechend.</akn:p>
               </akn:content>
            </akn:paragraph>
         </akn:article>
         <!-- Artikel 34: Geltungszeitregel-->
         <akn:article refersTo="geltungszeitregel"
                      GUID="24e27c42-ae14-4bc1-ade8-508f001cf1ff"
                      eId="art-z34"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1">
            <akn:num GUID="6d0a7dc2-d90a-4e24-8b16-f079c5d91e86"
                     eId="art-z34_bezeichnung-n1"> Artikel 34</akn:num>
            <akn:heading GUID="4b31cf37-357a-4396-ab76-96f90524e84f"
                         eId="art-z34_überschrift-n1">Inkrafttreten</akn:heading>
            <!-- Absatz (1) -->
            <akn:paragraph GUID="171e68a0-665e-47ea-8fca-ac496383c32e" eId="art-z34_abs-z">
               <akn:num GUID="b46dc31c-fef1-4f85-bb47-1113e5df7420"
                        eId="art-z34_abs-z_bezeichnung-n1"/>
               <akn:content GUID="780f3faa-5b04-49c5-83ce-83902186f289"
                            eId="art-z34_abs-z_inhalt-n1">
                  <akn:p GUID="c24ee1ad-34b2-4259-b8d4-1cd6fcaae107"
                         eId="art-z34_abs-z_inhalt-n1_text-n1">Dieses Gesetz tritt einen Monat nach seiner Verkündung in Kraft.</akn:p>
               </akn:content>
            </akn:paragraph>
         </akn:article>
      </akn:body>
      <akn:conclusions GUID="74e3022f-c711-4825-9b30-4b9a1bafd8d0" eId="schluss-n1">
