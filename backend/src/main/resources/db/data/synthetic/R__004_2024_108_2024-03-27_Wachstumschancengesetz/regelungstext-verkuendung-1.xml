            <akn:num eId="art-z%c2%a71_bezeichnung-n1"
                     GUID="24f5f556-e005-4d10-8d7e-d92f141780d7">
                    §1</akn:num>
            <akn:heading eId="art-z%c2%a71_überschrift-n1"
                         GUID="31b97e2a-1872-4a52-aba1-d075bae3c0d6">Änderung des Einkommensteuergesetzes</akn:heading>
            <!-- Absatz 1 -->
            <akn:paragraph eId="art-z%c2%a71_abs-z1" GUID="58ea2242-386a-42b9-8f23-eaa1678159f1">
               <akn:num eId="art-z%c2%a71_abs-z1_bezeichnung-n1"
                        GUID="c38a41af-4911-4b1a-9404-929912af983e">
                        (1)</akn:num>
               <akn:content GUID="dc2f76bf-0bf2-42c2-899e-aa52bd273925"
                            eId="art-z%c2%a71_abs-z1_inhalt-n1">
                  <akn:blockList GUID="5d6fc824-7926-43b4-b243-b0096da183f8"
                                 eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1">
                     <akn:listIntroduction GUID="5d6fc824-7926-43b4-b243-b0096da183f9"
                                           eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1_listeneing-n1">
                      Das <akn:affectedDocument GUID="e9e5ceb0-c831-45da-8fa4-aaaa16572bee"
                                              eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1_listeneing-n1_bezugsdoc-n1"
                                              href="eli/bund/bgbl-1/2009/s3366/2023-12-23/1/deu/regelungstext-1">Einkommensteuergesetz in der Fassung der Bekanntmachung vom 8. Oktober 2009 (BGBl. I S. 3366, 3862),
                                das zuletzt durch Artikel 20 des Gesetzes vom 22. Dezember 2023 (BGBl. 2023 I Nr. 411) geändert worden ist</akn:affectedDocument>, wird
                                wie folgt geändert:
                  </akn:listIntroduction>
                     <akn:item GUID="49983c1a-c952-4ab1-b926-2f414c05da7d"
                               eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1_listenelem-n1">
                        <akn:num GUID="634ddcaa-4851-4656-8b65-f26c8f56d1f1"
                                 eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1_listenelem-n1_bezeichnung-n1">
                                1.
                            </akn:num>
                        <akn:p eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1_listenelem-n1_text-n1"
                               GUID="a8786a31-bf59-41f9-b945-7785ed93861a">
                           <akn:mod refersTo="aenderungsbefehl-ersetzen"
                                    eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1"
                                    GUID="a8786a31-bf59-41f9-b945-7785ed93861b">
                                        In <akn:ref href="eli/bund/bgbl-1/2009/s3366/2023-12-23/1/deu/regelungstext-1/hauptteil-1_art-27_abs-6_inhalt-1_text-1/302-338.xml"
                                       eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_ref-n1"
                                       GUID="a8786a31-bf59-41f9-b945-7785ed93861c">§ 6b Absatz 6 Satz 2</akn:ref> werden die Wörter „<akn:quotedText startQuote="„"
                                              endQuote="“"
                                              eId="art-z%c2%a71_abs-z1_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quottext-n1"
