      <!-- Hauptteil -->
      <akn:body eId="hauptteil-n1" GUID="ce0ac10d-510e-4148-8a8d-a4406a8b0a67">
            <!-- §1 -->
         <akn:article eId="art-z%c2%a71"
                      GUID="17e9c25e-251d-48a7-8e74-3f0bb4b93b6b"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n2"
