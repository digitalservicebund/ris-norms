            <akn:paragraph GUID="025f5b72-d2eb-48ff-bb97-cda270b700c6" eId="art-z2_abs-z">
               <akn:num GUID="847ece9d-bfc9-4382-a14a-e5a40b93cb00"
                        eId="art-z2_abs-z_bezeichnung-n1"/>
               <akn:content GUID="f8a07fe3-deff-4373-b78e-0cc5c3014bc7"
                            eId="art-z2_abs-z_inhalt-n1">
                  <akn:p GUID="3e10fde0-5ece-405e-bc09-809bca823c0c"
                         eId="art-z2_abs-z_inhalt-n1_text-n1"> Dieses Gesetz tritt <akn:date date="1001-01-02"
                               refersTo="inkrafttreten-datum"
                               GUID="839c57b8-737d-4d52-b583-b44b61c2db63"
