      <!-- Hauptteil -->
      <akn:body eId="hauptteil-n1" GUID="2094abe2-16e6-44d5-9198-3ef9486833e9">
         <akn:article eId="art-z20"
                      GUID="b1b4bd3b-e007-4d84-af83-b8e36a0ae50b"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1">
            <akn:num eId="art-z20_bezeichnung-n1"
                     GUID="f82ab983-5498-49ab-918f-5cf5e730e5ec">
               § 20</akn:num>
            <akn:paragraph eId="art-z20_abs-z1"
                           GUID="6aa3a7ca-f30a-43b6-950b-b1e942fd1842"
                           period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1">
               <akn:num eId="art-z20_abs-z1_bezeichnung-n1"
                        GUID="e363f12d-7918-435c-b3a1-182c5e03ff43">
                  (1) </akn:num>
               <akn:list eId="art-z20_abs-z1_untergl-n1"
                         GUID="97e930bf-49f8-472a-a1fa-3c3a401caa13">
                  <akn:intro eId="art-z20_abs-z1_untergl-n1_intro-n1"
                             GUID="e2261ee5-feda-4691-b1a0-8a18f43720e7">
                     <akn:p eId="art-z20_abs-z1_untergl-n1_intro-n1_text-n1"
                            GUID="f5b78960-3376-4f19-a1de-05e24443a141">Wer</akn:p>
                  </akn:intro>
                  <akn:point eId="art-z20_abs-z1_untergl-n1_listenelem-n1"
                             GUID="6f1b22e6-2fcc-4e29-b3c1-fd67d8cee45c">
                     <akn:num eId="art-z20_abs-z1_untergl-n1_listenelem-n1_bezeichnung-n1"
                              GUID="7947817d-e127-49ca-82c3-a1dfeaa73748">
                        1. </akn:num>
                     <akn:content eId="art-z20_abs-z1_untergl-n1_listenelem-n1_inhalt-n1"
                                  GUID="fff000f9-bed8-41eb-b26b-93a5da40ff5f">
                        <akn:p eId="art-z20_abs-z1_untergl-n1_listenelem-n1_inhalt-n1_text-n1"
                               GUID="6ad07a0f-46f6-45b5-8d1a-ac79b8f704fe">entgegen einem
                                    vollziehbaren Verbot den Verein fortführt, seinen organisatorischen Zusammenhalt auf andere Weise aufrechterhält, sich an ihm als Mitglied
                           beteiligt, für ihn wirbt, ihn unterstützt oder eine Tätigkeit ausübt (§ 18 Satz 2) oder</akn:p>
                     </akn:content>
                  </akn:point>
                  <akn:point eId="art-z20_abs-z1_untergl-n1_listenelem-n2"
                             GUID="94af45e6-d49b-4bcc-84b9-5e5c01d5a3ba">
                     <akn:num eId="art-z20_abs-z1_untergl-n1_listenelem-n2_bezeichnung-n1"
                              GUID="18a496f3-c0e7-4fb3-a41d-5c9fd75374c7">
                        2. </akn:num>
                     <akn:content eId="art-z20_abs-z1_untergl-n1_listenelem-n2_inhalt-n1"
                                  GUID="7e835e4b-52eb-4fa1-9698-d7a42589d715">
                        <akn:p eId="art-z20_abs-z1_untergl-n1_listenelem-n2_inhalt-n1_text-n1"
                               GUID="0ba9a471-e9ef-44c4-b5da-f69f068a4483">entgegen § 9 Abs. 1 Satz 2, Abs. 2 Kennzeichen eines verbotenen Vereins oder einer Ersatzorganisation verwendet,</akn:p>
                     </akn:content>
                  </akn:point>
                  <akn:wrapUp eId="art-z20_abs-z1_untergl-n1_schlusstext-n1"
                              GUID="107ed2fd-e041-4ee4-9eee-b559f84a4ce8">
                     <akn:p eId="art-z20_abs-z1_untergl-n1_schlusstext-n1_text-n1"
                            GUID="53af6200-56c2-4fed-bd27-25ddefe7b204">wird mit Gefängnis bis zu einem Jahr oder mit Geldstrafe bestraft, wenn die Tat nicht in den §§ 49 b, 90 a, 90 b, 96 a, 128 oder 129 des Strafgesetzbuches, mit
                        schwerer Strafe bedroht ist.</akn:p>
                  </akn:wrapUp>
               </akn:list>
            </akn:paragraph>
            <akn:paragraph eId="art-z20_abs-z2"
                           GUID="f0f31024-8381-4a62-8980-5f432d03a925"
                           period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1">
               <akn:num eId="art-z20_abs-z2_bezeichnung-n1"
                        GUID="c0ea5d8c-8d91-4e0c-812e-429a7a24a1a2">
                  (2) </akn:num>
               <akn:content eId="art-z20_abs-z2_inhalt-n1"
                            GUID="8b51c358-ca54-4949-b957-bcbe5e55f33d">
                  <akn:p eId="art-z20_abs-z2_inhalt-n1_text-n1"
                         GUID="924d7140-bdfd-4a50-9b7f-feb006d19aae">In den Fällen des Absatzes 1 Nr. 1 gilt § 90 a Abs. 5 und 6 des Strafgesetzbuches entsprechend.</akn:p>
               </akn:content>
            </akn:paragraph>
         </akn:article>
         <!-- Artikel 34: Geltungszeitregel-->
         <akn:article eId="art-z34"
                      GUID="6574da6e-2a23-4280-80dd-b4f4c5416cdf"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1"
                      refersTo="geltungszeitregel">
            <akn:num eId="art-z34_bezeichnung-n1"
                     GUID="19b65c8b-44ae-4092-be09-7de64ac623e5">
               Artikel 34</akn:num>
            <akn:heading eId="art-z34_überschrift-n1"
                         GUID="772db065-a163-4453-a22f-269f6aebddaa">Inkrafttreten</akn:heading>
            <!-- Absatz (1) -->
            <akn:paragraph eId="art-z34_abs-z" GUID="38c00dca-43e1-4cb5-aa69-bd03cc486c99">
               <akn:num eId="art-z34_abs-z_bezeichnung-n1"
                        GUID="b5758ec8-bcb9-4b69-8764-fb86911e937c">
               </akn:num>
               <akn:content eId="art-z34_abs-z_inhalt-n1"
                            GUID="45ca26fa-b7c0-416b-9a0a-7f2bd7c86873">
                  <akn:p eId="art-z34_abs-z_inhalt-n1_text-n1"
                         GUID="aa475227-3bb4-4568-99b3-98467b8daf91">Dieses Gesetz tritt einen Monat nach seiner Verkündung in Kraft.</akn:p>
               </akn:content>
            </akn:paragraph>
         </akn:article>
