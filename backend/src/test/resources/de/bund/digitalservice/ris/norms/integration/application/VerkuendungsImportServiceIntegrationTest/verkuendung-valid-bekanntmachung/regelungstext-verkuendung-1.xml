                            refersTo="geltungszeitregel">
                  <akn:num GUID="77dd37b6-035d-3dd8-af31-1d73bb67e354"
                           eId="art-z%c2%a763_bezeichnung-n1">§63</akn:num>
                  <akn:heading GUID="b59bb612-f58a-31c3-8e59-0bb4f0e372da"
                               eId="art-z%c2%a763_überschrift-n1">(Inkrafttreten)</akn:heading>
                  <akn:paragraph GUID="dc0d3889-80c6-3c17-be4f-639a3346dd2b" eId="art-z%c2%a763_abs-z">
                     <akn:num GUID="e95ff9c8-264d-3f5a-9730-422309b7657b"
                              eId="art-z%c2%a763_abs-z_bezeichnung-n1">
<!-- ohne Nummierung! --></akn:num>
                     <akn:content GUID="61fa9e55-10cd-36d9-a1c2-9e47f4352b0f"
                                  eId="art-z%c2%a763_abs-z_inhalt-n1">
                        <akn:p GUID="57b44714-0383-3eac-a89c-8f58a017ac80"
                               eId="art-z%c2%a763_abs-z_inhalt-n1_text-n1"/>
                     </akn:content>
                  </akn:paragraph>
               </akn:article>
            </akn:subsection>
         </akn:section>
