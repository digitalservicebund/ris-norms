      <akn:body eId="hauptteil-1" GUID="0B4A8E1F-65EF-4B7C-9E22-E83BA6B73CD8">
            <!-- Artikel 1: Hauptänderung -->
         <akn:article eId="hauptteil-1_art-1"
                      GUID="e61fdbd6-eb6f-4196-8cff-2fb9431918b4"
                      period="#meta-1_geltzeiten-1_geltungszeitgr-1"
                      refersTo="hauptaenderung">
            <akn:num eId="hauptteil-1_art-1_bezeichnung-1"
                     GUID="109ff2d8-e10a-42c8-b17b-8d7e733dbda2">
                    Artikel 1
                </akn:num>
            <akn:heading eId="hauptteil-1_art-1_überschrift-1"
                         GUID="04529a28-27a3-4ec4-a8b6-6561a99c26e5">Änderung des Vereinsgesetzes</akn:heading>
            <!-- Absatz (1) -->
            <akn:paragraph eId="hauptteil-1_art-1_abs-1"
                           GUID="5f6fba05-6067-4165-b9fe-729fd3f3c72d">
               <akn:num eId="hauptteil-1_art-1_abs-1_bezeichnung-1"
                        GUID="16422084-53d2-4a91-af63-b6a708cd1f1a">
                    </akn:num>
               <akn:list eId="hauptteil-1_art-1_abs-1_untergl-1"
                         GUID="ae09d3ec-0ab0-4ea4-8038-d189c599213a">
                  <akn:intro eId="hauptteil-1_art-1_abs-1_untergl-1_intro-1"
                             GUID="72307b70-0cbb-43c9-a0ec-f5b36e372929">
                     <akn:p eId="hauptteil-1_art-1_abs-1_untergl-1_intro-1_text-1"
                            GUID="35a2dbd4-e758-461a-8e41-e8f0491c0a32">Das <akn:affectedDocument
                             eId="hauptteil-1_art-1_abs-1_untergl-1_intro-1_text-1_bezugsdoc-1"
                             GUID="e616cf30-e1ff-42d4-a475-a651cd257a42"
                             href="eli/bund/bgbl-1/1964/s593/1964-08-05/1/deu/regelungstext-1">Vereinsgesetz vom
                                5. August 1964 (BGBl. I S. 593), das zuletzt
                                durch … geändert worden ist</akn:affectedDocument>, wird wie folgt geändert:
                            </akn:p>
                  </akn:intro>
                  <!-- Nummer 2 -->
                  <akn:point eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1"
                             GUID="44b176c2-854a-4f22-b516-787cc6c4038d">
                     <akn:num eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_bezeichnung-1"
                              GUID="79658c62-922e-4b9e-8626-bb6e5ba67451">
                                2.
                            </akn:num>
                     <akn:content eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1"
                                  GUID="aa25a858-57d1-43e3-af83-5208dd01bd5a">
                        <akn:p eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1"
                               GUID="3dd0b3e6-94c4-4f1e-b9f2-2dc8fd0deb6d">
                           <akn:mod eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1"
                                    GUID="79d0da46-f7fb-4a29-969f-afcc096ee93e"
                                    refersTo="aenderungsbefehl-ersetzen">In <akn:ref
                                   eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_ref-1"
                                   GUID="f1b7e8c6-3954-4c0c-b93a-505eceb1f662"
                                   href="eli/bund/bgbl-1/1964/s593/1964-08-05/1/deu/regelungstext-1/hauptteil-1_art-1_abs-1_untergl-1_listenelem-2_inhalt-1_text-1/9-34.xml">
                                        § 20 Absatz 1 Satz 2
                                    </akn:ref> wird
                                        die Angabe <akn:quotedText
                                      eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quottext-1"
                                      GUID="6cc86568-08c9-40ac-9c96-d62e576e25f7"
                                      startQuote="„"
                                      endQuote="“">§ 9 Abs. 1 Satz 2, Abs. 2
                                        </akn:quotedText> durch die
                                        Wörter
                                        <akn:quotedText
                                                eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quottext-2"
                                                GUID="cbf8bb1d-9aa5-4bc7-85f1-e51fe1b22b17"
                                                startQuote="„"
                                                endQuote="“">§ 9 Absatz 1 Satz 2, Absatz 2 oder 3
                                        </akn:quotedText>
                                        ersetzt.
                                    </akn:mod>
                        </akn:p>
                     </akn:content>
                  </akn:point>
               </akn:list>
            </akn:paragraph>
         </akn:article>
         <!-- Artikel 3: Geltungszeitregel-->
         <akn:article eId="hauptteil-1_art-2"
                      GUID="4d4b849a-655e-4f18-a639-1f34f77ff87c"
                      period="#geltungszeitgr-1"
                      refersTo="geltungszeitregel">
            <akn:num eId="hauptteil-1_art-2_bezeichnung-1"
                     GUID="b726a7d3-c8fa-4d00-a34c-3d6a5121df4a">
                    Artikel 3</akn:num>
            <akn:heading eId="hauptteil-1_art-2_überschrift-1"
                         GUID="390cef3d-13c2-480d-80e0-3c8ebc102287">Inkrafttreten</akn:heading>
            <!-- Absatz (1) -->
            <akn:paragraph eId="hauptteil-1_art-2_abs-1"
                           GUID="e4752fea-b440-401f-af7c-81d1b0036df1">
               <akn:num eId="hauptteil-1_art-2_abs-1_bezeichnung-1"
                        GUID="cd3c99aa-62eb-4954-a543-bd38a354f982">
                    </akn:num>
               <akn:content eId="hauptteil-1_art-2_abs-1_inhalt-1"
                            GUID="fa4ebe88-3db3-4820-a5f1-97b7fc242cd6">
                  <akn:p eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1"
                         GUID="3759029f-be39-4c26-9c68-d198b05443ae"> Dieses Gesetz tritt <akn:date
                          eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1_datum-1"
                          GUID="d52b79a9-8ef1-4dda-aa37-eb0614c69551"
                          date="2017-03-16"
                          refersTo="inkrafttreten-datum">am Tag nach der Verkündung</akn:date> in Kraft. </akn:p>
               </akn:content>
            </akn:paragraph>
            <akn:paragraph eId="hauptteil-1_art-2_abs-2" GUID="3665e313-3121-4365-a89e-cadeac4cbd0f">
               <akn:num eId="hauptteil-1_art-2_abs-2_bezeichnung-1" GUID="8bfdeb21-ed08-43f3-a35e-097bab41e907">
                  Absatz 2
               </akn:num>
               <akn:content eId="hauptteil-1_art-2_abs-2_inhalt-1" GUID="b23d0f38-0975-4e3b-a4ca-6e957f5f0ceb">
                  <akn:p eId="hauptteil-1_art-2_abs-2_inhalt-1_text-1" GUID="dc102936-c870-4289-a64a-2390a829e24a">
                     Weiterer Artikel
                  </akn:p>
               </akn:content>
