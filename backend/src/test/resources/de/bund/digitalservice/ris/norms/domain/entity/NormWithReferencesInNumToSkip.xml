                              eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_bezeichnung-1">
                                8.
                            </akn:num>
                     <akn:content GUID="2fe9739e-c809-4139-91fa-e111a7674e5b"
                                  eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1">
                        <akn:p GUID="5675e613-f44b-4383-a825-1da8673206ae"
                               eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1">
                           <akn:mod refersTo="aenderungsbefehl-ersetzen"
                                    GUID="2cbbf69b-9fe9-4996-b989-c10481119472"
                                    eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1">Das
                                        <akn:ref
                                                href="eli/bund/bgbl-1/1002/1/1002-01-01/1/deu/regelungstext-1/hauptteil-1_buch-3.xml"
                                                GUID="908f56a9-218c-478f-8bf4-7f915bf6ea59"
                                                eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_ref-1">3. Buch</akn:ref>
                                        wird ersetzt durch:
                                        <akn:quotedStructure startQuote="„"
                                                             endQuote="“"
                                                             GUID="5287afa2-cdd1-4d8c-b027-8ca653aa8f19"
                                                             eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1">
                                 <akn:book GUID="bff63266-ee36-4d78-9110-675239af599f"
                                           eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1">
                                    <akn:num GUID="ddec4bf9-36d3-4af4-a607-a0acb1ee1bea"
                                             eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_bezeichnung-1">
                                                    3. Buch
                                                </akn:num>
                                    <akn:heading GUID="0384394e-3c9f-4355-8580-70280688c64f"
                                                 eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_überschrift-1">Beispiele Teil I</akn:heading>
                                    <akn:article GUID="c31f4462-545e-4064-af55-d42bb34dacde"
                                                 eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_art-1"
                                                 period="#meta-1_geltzeiten-1_geltungszeitgr-2">
                                       <akn:num GUID="6b1085ed-6ac5-4ff3-ac2c-79602af6446f"
                                                eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_art-1_bezeichnung-1">
                                                        § 6
                                                    </akn:num>
                                       <akn:heading GUID="e3f17909-13f5-47e0-8467-e4906b90be2b"
                                                    eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_art-1_überschrift-1">Beispielartikel (neu)</akn:heading>
                                       <akn:paragraph GUID="d4cfcdd1-d1fd-408a-bf51-0720c852f6b5"
                                                      eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_art-1_abs-1">
                                          <akn:num GUID="9819cc84-b783-43e7-8185-b00f73748bdd"
                                                   eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_art-1_abs-1_bezeichnung-1">
                                                        </akn:num>
                                          <akn:content GUID="debcb0c7-991d-4823-9397-d55544fb70b0"
                                                       eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_art-1_abs-1_inhalt-1">
                                             <akn:p GUID="f6aa96b1-0d31-4b72-8651-5a6bc2b3b1c8"
                                                    eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quotstruct-1_buch-1_art-1_abs-1_inhalt-1_text-1">Im Rahmen des Gesetzes werden klare Regelungen für die Inanspruchnahme von Rechtsmitteln festgelegt.</akn:p>
                                          </akn:content>
                                       </akn:paragraph>
                                    </akn:article>
                                 </akn:book>
                              </akn:quotedStructure>
                           </akn:mod>
                        </akn:p>
                     </akn:content>
                  </akn:point>
                  <akn:point GUID="0f0384e5-311b-4db2-9d10-740a5450bad0"
                             eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-2">
                     <akn:num GUID="d88a6962-f945-4461-9921-06c43a32248d"
                              eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-2_bezeichnung-1">
                                9.
                            </akn:num>
                     <akn:content GUID="13d7f1c1-8ee4-4a44-8578-9a332ffa165d"
