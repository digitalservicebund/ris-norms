                                                    <akn:paragraph GUID="a0abdae7-7930-4f36-811d-86838bd5fcc8" eId="hauptteil-1_para-1_abs-1_untergl-1_listenelem-2_inhalt-1_text-1_ändbefehl-1_quotstruct-1_kapitel-1_para-7_abs-1">
