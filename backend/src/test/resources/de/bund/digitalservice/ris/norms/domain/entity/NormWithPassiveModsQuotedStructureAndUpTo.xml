                <akn:num GUID="11b74348-a016-45de-bf6b-4ec15bc96a96" eId="hauptteil-1_para-1_bezeichnung-1">
                    <akn:marker GUID="ba3fa73a-2f79-486e-b238-d69a7e534d5a" eId="hauptteil-1_para-1_bezeichnung-1_zaehlbez-1" name="1"/>§ 1
                </akn:num>
                <akn:heading GUID="adbfd292-9813-44c8-be67-4f2a57810e15" eId="hauptteil-1_para-1_überschrift-1">Gesetz zur Einbindung eines Regelungstextes in Stammform</akn:heading>
