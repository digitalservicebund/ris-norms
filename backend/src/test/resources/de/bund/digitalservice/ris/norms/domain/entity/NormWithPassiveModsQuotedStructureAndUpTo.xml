            <akn:num GUID="11b74348-a016-45de-bf6b-4ec15bc96a96"
                     eId="hauptteil-1_para-1_bezeichnung-1">
                    § 1
                </akn:num>
            <akn:heading GUID="adbfd292-9813-44c8-be67-4f2a57810e15"
                         eId="hauptteil-1_para-1_überschrift-1">Gesetz zur Einbindung eines Regelungstextes in Stammform</akn:heading>
            <akn:componentRef GUID="45725af6-3346-4828-980d-9e010ce4abd3"
                              eId="hauptteil-1_para-1_stfmverweis-1"
                              showAs="regelungstext-eingebundene-stammform"
                              src="01-06_instanz_01_regelungstext-eingebundene-stammform.xml"/>
         </akn:article>
         <akn:article GUID="bfc6170d-9ae5-49e6-bae9-c1255efe03b6"
                      eId="hauptteil-1_para-2"
                      period="#meta-1_geltzeiten-1_geltungszeitgr-1">
            <akn:num GUID="568e52b1-eeee-46d6-b5f0-c81a677fb275"
                     eId="hauptteil-1_para-2_bezeichnung-1">
                     § 2
                </akn:num>
            <akn:heading GUID="85ad062d-4e0e-4730-9131-c6c89752a732"
                         eId="hauptteil-1_para-2_überschrift-1"> Anwendung in Absätzen und Aufzählungen </akn:heading>
            <akn:paragraph GUID="cbf61857-bcd3-4ca3-b3f2-20e7ad0b8661"
                           eId="hauptteil-1_para-2_abs-1">
               <akn:num GUID="fbd88274-37a4-43c5-b821-ef4b71b2ce80"
                        eId="hauptteil-1_para-2_abs-1_bezeichnung-1">
                        (1)
                    </akn:num>
               <akn:content GUID="d9776e83-53f7-4363-8aee-56dd2130e85b"
                            eId="hauptteil-1_para-2_abs-1_inhalt-1">
                  <akn:p GUID="70440b91-57ac-4951-8663-695ff0966f19"
                         eId="hauptteil-1_para-2_abs-1_inhalt-1_text-1">Dieses Gesetz findet Anwendung auf alle definierten Struktur und Gliederungsebenen.</akn:p>
               </akn:content>
            </akn:paragraph>
            <akn:paragraph GUID="f0730675-7e3f-45bb-a2ee-46c1d89ffd94"
                           eId="hauptteil-1_para-2_abs-2">
               <akn:num GUID="29201137-5d19-46bf-96ba-ee0855731641"
                        eId="hauptteil-1_para-2_abs-2_bezeichnung-1">
                        (2)
                    </akn:num>
               <akn:content GUID="1d16bd72-6671-4127-afd1-ed574c0260b9"
                            eId="hauptteil-1_para-2_abs-2_inhalt-1">
                  <akn:p GUID="6c34ea8a-808c-4f63-a233-348fdba174b8"
                         eId="hauptteil-1_para-2_abs-2_inhalt-1_text-1">Die Berechnung der Anwendung erfolgt ohne Mathematik. </akn:p>
               </akn:content>
            </akn:paragraph>
            <akn:paragraph GUID="9707ddb3-6b7d-4d38-84fa-6f3d38aa6cd6"
                           eId="hauptteil-1_para-2_abs-3">
               <akn:num GUID="e88eccd4-3982-40f9-859c-a1dbeb041049"
                        eId="hauptteil-1_para-2_abs-3_bezeichnung-1">
                        (3)
                    </akn:num>
               <akn:list GUID="77d313f3-e315-4e7f-be96-425910ecfce9"
                         eId="hauptteil-1_para-2_abs-3_untergl-1">
                  <akn:intro GUID="22ebfacf-261e-4eee-bedb-abbbd4d6b1ff"
                             eId="hauptteil-1_para-2_abs-3_untergl-1_intro-1">
                     <akn:p GUID="fdfd2276-28cb-4024-9c84-2e91df5195fb"
                            eId="hauptteil-1_para-2_abs-3_untergl-1_intro-1_text-1">Die Bestimmungen dieses Gesetzes gelten insbesondere für:</akn:p>
                  </akn:intro>
                  <akn:point GUID="42f4f096-fba5-4b1a-bc4d-6121f65df406"
                             eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1">
                     <akn:num GUID="1d789e82-5784-4be9-9073-d7b48ebef194"
                              eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_bezeichnung-1">
                                1.
                            </akn:num>
                     <akn:list GUID="c8e81179-ed0b-4bfe-8d3c-6be1e6033c16"
                               eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1">
                        <akn:intro GUID="2348ae40-4138-47a7-927a-ea372fa6ac38"
                                   eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_intro-1">
                           <akn:p GUID="7f2242ae-101e-47b9-b5d2-56744245d60a"
                                  eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_intro-1_text-1">Gliederungen im Regelungstext wie zum Beispiel</akn:p>
                        </akn:intro>
                        <akn:point GUID="30fb6904-1965-49fc-a829-f7821c8b2291"
                                   eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-1">
                           <akn:num GUID="2c944bf4-f610-425a-b0bf-fc14211bad91"
                                    eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-1_bezeichnung-1">
                                        a)
                                    </akn:num>
                           <akn:content GUID="c5d96ead-a686-4179-8c70-5f5c53af0934"
                                        eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-1_inhalt-1">
                              <akn:p GUID="4e3a68e1-a6dc-48a4-85ee-692473dd6231"
                                     eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-1_inhalt-1_text-1"> Bücher, </akn:p>
                           </akn:content>
                        </akn:point>
                        <akn:point GUID="a847f9da-9a41-4d4e-beab-874fb6beec7f"
                                   eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-2">
                           <akn:num GUID="a0419aca-7a16-4cb3-a57d-003ed60a4e69"
                                    eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-2_bezeichnung-1">
                                        b)
                                    </akn:num>
                           <akn:content GUID="2746d6e9-1b49-43fb-9054-0bc34c0ac0f2"
                                        eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-2_inhalt-1">
                              <akn:p GUID="e15cee5a-8ec9-4eee-afbe-2ee07c8fc5f9"
                                     eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-2_inhalt-1_text-1"> Teile, </akn:p>
                           </akn:content>
                        </akn:point>
                        <akn:point GUID="a878aadd-56bd-4c34-86ba-a0849c7bfbf3"
                                   eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-3">
                           <akn:num GUID="3c013250-64a7-4535-b757-3c6ac877cb94"
                                    eId="hauptteil-1_para-2_abs-3_untergl-1_listenelem-1_untergl-1_listenelem-3_bezeichnung-1">
                                        c)
