            </akn:article>
            <akn:article GUID="bfc6170d-9ae5-49e6-bae9-c1255efe03b6" eId="hauptteil-1_para-2" period="#meta-1_geltzeiten-1_geltungszeitgr-1">
                <akn:num GUID="568e52b1-eeee-46d6-b5f0-c81a677fb275" eId="hauptteil-1_para-2_bezeichnung-1">
