                        eId="hauptteil-1_para-2_abs-3_bezeichnung-1">
                        (3) </akn:num>
               <akn:list GUID="77d313f3-e315-4e7f-be96-425910ecfce9"
                         eId="hauptteil-1_para-2_abs-3_untergl-1">
                  <akn:intro GUID="22ebfacf-261e-4eee-bedb-abbbd4d6b1ff"
                             eId="hauptteil-1_para-2_abs-3_untergl-1_intro-1">
                     <akn:p GUID="fdfd2276-28cb-4024-9c84-2e91df5195fb"
                            eId="hauptteil-1_para-2_abs-3_untergl-1_intro-1_text-1">Die Bestimmungen dieses Gesetzes gelten insbesondere für:</akn:p>
                  </akn:intro>
