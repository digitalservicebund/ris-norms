                     § 2 </akn:num>
            <akn:heading GUID="85ad062d-4e0e-4730-9131-c6c89752a732"
                         eId="hauptteil-1_para-2_überschrift-1"> Anwendung in Absätzen und Aufzählungen </akn:heading>
            <akn:paragraph GUID="69243922-59f2-4733-b590-55d7e8cfdf03"
                           eId="hauptteil-1_para-2_abs-1">
               <akn:num GUID="2626b5a4-2b15-43d1-9297-3949099ca6dc"
                        eId="hauptteil-1_para-2_abs-1_bezeichnung-1">
                        (1)</akn:num>
               <akn:content GUID="f5e3fa13-3ed9-41c4-a4ee-91db57abdd51"
                            eId="hauptteil-1_para-2_abs-1_inhalt-1">
                  <akn:p GUID="fabee05a-1bba-4898-bd30-3472717fc013"
                         eId="hauptteil-1_para-2_abs-1_inhalt-1_text-1">Dieses Gesetz findet keine Anwendung.</akn:p>
               </akn:content>
            </akn:paragraph>
            <akn:paragraph GUID="0c404488-4159-4756-8467-2400daa1c489"
                           eId="hauptteil-1_para-2_abs-2">
