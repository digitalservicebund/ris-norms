      <akn:body eId="hauptteil-1" GUID="fc0a61ec-81c4-461a-a961-0c23a60bc8d5">
            <!-- § 9 -->
         <akn:article eId="hauptteil-1_art-1"
                      GUID="8d497e15-eeda-44ac-bca7-d45976c594e9"
                      period="#geltungszeitgr-1">
            <akn:num eId="hauptteil-1_art-1_bezeichnung-1"
                     GUID="e05944fd-7ad2-4e66-bce8-4576939c02bf">
                    § 20
                </akn:num>
            <akn:paragraph eId="hauptteil-1_art-1_abs-1"
                           GUID="bce27cf8-5b23-4a12-8f0c-25736190bbf2"
                           period="#geltungszeitgr-2">
               <akn:num eId="hauptteil-1_art-1_abs-1_bezeichnung-1"
                        GUID="95c5bb7d-ac77-4444-b682-343b1b798856">
                        (1)
                    </akn:num>
               <akn:list eId="hauptteil-1_art-1_abs-1_untergl-1"
                         GUID="1038b139-5365-4cd1-8d9e-ac77a7ed2e67">
                  <akn:intro eId="hauptteil-1_art-1_abs-1_untergl-1_intro-1"
                             GUID="035cb42b-6fe4-482b-a730-d47f3bfc5073">
                     <akn:p eId="hauptteil-1_art-1_abs-1_untergl-1_intro-1_text-1"
                            GUID="6f9c72de-8b32-40aa-9647-630202ed470b">Wer
                            </akn:p>
                  </akn:intro>
                  <akn:point eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1"
                             GUID="136c6ef6-c8db-4283-bdf5-2e862833ee60">
                     <akn:num eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_bezeichnung-1"
                              GUID="241b7607-0905-46ae-9fe1-ccd4b4d4b158">
                                1.
                            </akn:num>
                     <akn:content eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1"
                                  GUID="3c812cbb-0299-4809-a0fc-9a64957d5456">
                        <akn:p eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1"
                               GUID="721ec4c9-29b6-4350-b296-95bd986c1289">entgegen einem
                                    vollziehbaren Verbot den Verein fortführt, seinen organisatorischen Zusammenhalt auf
                                    andere Weise aufrechterhält, sich an ihm als
                                    Mitglied beteiligt, für ihn wirbt, ihn unterstützt oder eine Tätigkeit ausübt (§ 18
                                    Satz 2) oder
                                </akn:p>
                     </akn:content>
                  </akn:point>
                  <akn:point eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-2"
                             GUID="d762da96-9e1d-4e6b-bd2a-005b2f7fcff7">
                     <akn:num eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-2_bezeichnung-1"
                              GUID="ebf6051d-4673-46e6-a30b-5d62a45e3871">
                                2.
                            </akn:num>
                     <akn:content eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-2_inhalt-1"
                                  GUID="1e109463-6fd1-4577-a691-6a36f9ee33fe">
                        <akn:p eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-2_inhalt-1_text-1"
                               GUID="c4610ab1-9589-4d9c-94a7-67750e9717d1">entgegen § 9 Abs. 1 Satz 2, Abs. 2
                                    Kennezichen eines verbotenen Vereins oder einer Ersatzorganisation verwendet,
                                </akn:p>
                     </akn:content>
                  </akn:point>
                  <akn:wrapUp eId="hauptteil-1_art-1_abs-1_untergl-1_schlusstext-1"
                              GUID="22898ba7-e1e7-484b-a45f-0ca81f3c958b">
                     <akn:p eId="hauptteil-1_art-1_abs-1_untergl-1_schlusstext-1_text-1"
                            GUID="26a09ec6-6798-4571-b4c0-feb7115f169a">wird mit Gefängnis bis zu
                                einem Jahr oder mit Geldstrafe bestraft, wenn die Tat nicht in den §§ 49 b, 90 a, 90 b,
                                96 a, 128 oder 129 des Strafgesetzbuches, mit
                                schwerer Strafe bedroht ist.
                            </akn:p>
                  </akn:wrapUp>
               </akn:list>
            </akn:paragraph>
            </akn:article>
              <!-- Artikel 34: Geltungszeitregel-->
           <akn:article eId="hauptteil-1_art-2"
                        GUID="6574da6e-2a23-4280-80dd-b4f4c5416cdf"
                        period="#geltungszeitgr-1"
                        refersTo="geltungszeitregel">
            <akn:num eId="hauptteil-1_art-2_bezeichnung-1"
                     GUID="19b65c8b-44ae-4092-be09-7de64ac623e5">
               Artikel 34</akn:num>
            <akn:heading eId="hauptteil-1_art-2_überschrift-1"
                         GUID="772db065-a163-4453-a22f-269f6aebddaa">Inkrafttreten</akn:heading>
               <!-- Absatz (1) -->
               <akn:paragraph eId="hauptteil-1_art-2_abs-1"
                              GUID="38c00dca-43e1-4cb5-aa69-bd03cc486c99">
               <akn:num eId="hauptteil-1_art-2_abs-1_bezeichnung-1"
                        GUID="b5758ec8-bcb9-4b69-8764-fb86911e937c">
               </akn:num>
               <akn:content eId="hauptteil-1_art-2_abs-1_inhalt-1"
                            GUID="45ca26fa-b7c0-416b-9a0a-7f2bd7c86873">
                  <akn:p eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1"
                         GUID="aa475227-3bb4-4568-99b3-98467b8daf91">Dieses Gesetz tritt einen Monat nach seiner Verkündung in Kraft.</akn:p>
               </akn:content>
            </akn:paragraph>
         </akn:article>
      </akn:body>
