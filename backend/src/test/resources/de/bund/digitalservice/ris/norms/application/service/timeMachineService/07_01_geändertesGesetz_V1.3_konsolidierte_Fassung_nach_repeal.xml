                      name="attributsemantik-noch-undefiniert">
            <akn:p eId="schluss-1_formel-1_text-1"
                   GUID="332b89f7-7f46-43eb-b660-9b325ed52b57">Das vorstehende Gesetz wird hiermit verkündet.</akn:p>
         </akn:formula>
