<!-- § 9 -->
         <akn:article eId="hauptteil-1_para-9"
                      GUID="b2390631-4ed0-45f6-af24-dd4deeaeae06"
                      period="#geltungszeitgr-1">
            <akn:num eId="hauptteil-1_para-9_bezeichnung-1"
