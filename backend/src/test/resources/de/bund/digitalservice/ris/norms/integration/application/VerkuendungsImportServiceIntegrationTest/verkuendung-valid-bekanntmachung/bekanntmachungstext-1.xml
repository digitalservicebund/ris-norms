      <akn:mainBody GUID="bc19945d-1510-32fe-9e8c-fd53d8aefdde" eId="hauptteil-n1">
         <akn:blockList GUID="359b9a54-e273-35e8-b6d8-e67fd1776d90" eId="hauptteil-n1_liste-n1">
            <akn:listIntroduction GUID="43edf49b-9bca-3025-b9e0-eedb3ddc68a8"
                                  eId="hauptteil-n1_liste-n1_listeneing-n1">Auf Grund des<akn:ref GUID="40e9da2d-044a-3ed4-959f-e3aa3cffd2d1"
                        eId="hauptteil-n1_liste-n1_listeneing-n1_ref-n1"
                        href="">Artikels 2 des Dritten Gesetzes zur Änderung des Saatgutverkehrsgesetzes vom 30. Januar 2004 (BGBl. I S. 126)</akn:ref>wird nachstehend der Wortlaut des<akn:ref GUID="4b818b85-4440-3dd3-9c0f-cd6d4e6f3c0b"
                        eId="hauptteil-n1_liste-n1_listeneing-n1_ref-n2"
                        href="">Saatgutverkehrsgesetzes</akn:ref>in der seit dem 13. Februar 2004 geltenden Fassung bekannt gemacht. Die Neufassung berücksichtigt:</akn:listIntroduction>
            <akn:item GUID="8fabc398-2ba5-37a9-98b0-5645339c424a"
                      eId="hauptteil-n1_liste-n1_listenelem-n1">
               <akn:num GUID="b5e843a8-6178-36ad-b38f-420c0cad88e1"
                        eId="hauptteil-n1_liste-n1_listenelem-n1_bezeichnung-n1">1.</akn:num>
               <akn:p GUID="8cacbc55-fbcb-3cdc-aa1c-6e13515eff26"
                      eId="hauptteil-n1_liste-n1_listenelem-n1_text-n1">das am 24. August 1985 in Kraft getretene<akn:ref GUID="865725af-031a-3b5d-a91a-24019b0929fb"
                           eId="hauptteil-n1_liste-n1_listenelem-n1_text-n1_ref-n1"
                           href="">Gesetz vom 20. August 1985 (BGBl. I S. 1633)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="49fcc66a-0cde-3f44-a86b-b41591ea446e"
                      eId="hauptteil-n1_liste-n1_listenelem-n2">
               <akn:num GUID="59b599c4-f721-3975-a2c3-b52ef7bb53c9"
                        eId="hauptteil-n1_liste-n1_listenelem-n2_bezeichnung-n1">2.</akn:num>
               <akn:p GUID="495ed1dd-2719-3588-bb46-c925612d4096"
                      eId="hauptteil-n1_liste-n1_listenelem-n2_text-n1">den am 1. Juli 1990 in Kraft getretenen<akn:ref GUID="c9368790-45c0-3b7e-8986-d3c9c6b43c10"
                           eId="hauptteil-n1_liste-n1_listenelem-n2_text-n1_ref-n1"
                           href="">Artikel 14 des Gesetzes vom 28. Juni 1990 (BGBl. I S. 1221)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="13191b97-42e5-3f03-afdf-8e992d1d5f89"
                      eId="hauptteil-n1_liste-n1_listenelem-n3">
               <akn:num GUID="1bc89ca6-0aa7-3831-afe5-379790d03ebf"
                        eId="hauptteil-n1_liste-n1_listenelem-n3_bezeichnung-n1">3.</akn:num>
               <akn:p GUID="8afe1ecf-8606-3aac-93ce-588ff1e7bc03"
                      eId="hauptteil-n1_liste-n1_listenelem-n3_text-n1">den am 31. Juli 1992 in Kraft getretenen<akn:ref GUID="38240619-ed73-30b3-9a49-e61a83c54cb3"
                           eId="hauptteil-n1_liste-n1_listenelem-n3_text-n1_ref-n1"
                           href="">Artikel 1 des Gesetzes vom 23. Juli 1992 (BGBl. I S. 1367)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="8644c1cf-73ef-3d18-bf15-42e374e05812"
                      eId="hauptteil-n1_liste-n1_listenelem-n4">
               <akn:num GUID="03ab5b96-2f55-3ccf-b6b2-37e8dc153de8"
                        eId="hauptteil-n1_liste-n1_listenelem-n4_bezeichnung-n1">4.</akn:num>
               <akn:p GUID="6e5c162f-53e6-3fdc-8672-9e9e416a2205"
                      eId="hauptteil-n1_liste-n1_listenelem-n4_text-n1">den am 1. Januar 1994 in Kraft getretenen<akn:ref GUID="0a47aa10-0600-3910-9f6e-c3de11ba93ba"
                           eId="hauptteil-n1_liste-n1_listenelem-n4_text-n1_ref-n1"
                           href="">Artikel 69 des Gesetzes vom 27. April 1993 (BGBl. I S. 512, 2436)</akn:ref>, dieser wiederum geändert durch den am 3. Dezember 1993 in Kraft getretenen<akn:ref GUID="ff43c89c-9e34-39b3-abf0-44abf907e88a"
                           eId="hauptteil-n1_liste-n1_listenelem-n4_text-n1_ref-n2"
                           href="">Artikel 3 des Gesetzes vom 25. November 1993 (BGBl. I S. 1917)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="68a8d7bb-2cb9-3efc-bb4d-55e1744fe20e"
                      eId="hauptteil-n1_liste-n1_listenelem-n5">
               <akn:num GUID="9ed69083-7714-3f54-9a0a-73522fb2b13b"
                        eId="hauptteil-n1_liste-n1_listenelem-n5_bezeichnung-n1">5.</akn:num>
               <akn:p GUID="8a3c9038-bfa0-31e6-92de-efe20de2351a"
                      eId="hauptteil-n1_liste-n1_listenelem-n5_text-n1">den am 3. Dezember 1993 in Kraft getretenen<akn:ref GUID="ee6c6e98-1ce7-3187-9f0d-76da6114496d"
                           eId="hauptteil-n1_liste-n1_listenelem-n5_text-n1_ref-n1"
                           href="">Artikel 2 des Gesetzes vom 25. November 1993 (BGBl. I S. 1917)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="de5ec67c-f531-3915-8b91-78b06c2bf063"
                      eId="hauptteil-n1_liste-n1_listenelem-n6">
               <akn:num GUID="77db55ab-a2ae-3174-b8bd-36edba16a400"
                        eId="hauptteil-n1_liste-n1_listenelem-n6_bezeichnung-n1">6.</akn:num>
               <akn:p GUID="13bd11fc-86d2-37a1-8058-cb368e3b5c03"
                      eId="hauptteil-n1_liste-n1_listenelem-n6_text-n1">den am 1. Januar 1995 in Kraft getretenen<akn:ref GUID="3d59c7d2-d779-3dc6-95ab-46516325ba2b"
                           eId="hauptteil-n1_liste-n1_listenelem-n6_text-n1_ref-n1"
                           href="">Artikel 12 des Gesetzes vom 2. August 1994 (BGBl. I S. 2018)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="5d5c4a04-a176-3990-9b13-bbb8057563a1"
                      eId="hauptteil-n1_liste-n1_listenelem-n7">
               <akn:num GUID="4917b3f6-6c70-3cfe-a868-208f381ad69a"
                        eId="hauptteil-n1_liste-n1_listenelem-n7_bezeichnung-n1">7.</akn:num>
               <akn:p GUID="1a0ebbd4-14f7-3ed2-97ad-cdd890d69548"
                      eId="hauptteil-n1_liste-n1_listenelem-n7_text-n1">den am 1. Januar 1995 in Kraft getretenen<akn:ref GUID="b808fec9-0cf0-397a-9d43-2776159940b1"
                           eId="hauptteil-n1_liste-n1_listenelem-n7_text-n1_ref-n1"
                           href="">Artikel 39 des Gesetzes vom 25. Oktober 1994 (BGBl. I S. 3082)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="57d4b499-3e54-3d69-98de-078844399877"
                      eId="hauptteil-n1_liste-n1_listenelem-n8">
               <akn:num GUID="8f0c4ae4-aa13-3ae7-91c0-bcf8ea3469fa"
                        eId="hauptteil-n1_liste-n1_listenelem-n8_bezeichnung-n1">8.</akn:num>
               <akn:p GUID="3d12710e-f64b-3745-b85e-760c939fcdb6"
                      eId="hauptteil-n1_liste-n1_listenelem-n8_text-n1">den am 28. Juni 2001 in Kraft getretenen<akn:ref GUID="44bdc3fe-fe15-341d-817a-d157d8dcda05"
                           eId="hauptteil-n1_liste-n1_listenelem-n8_text-n1_ref-n1"
                           href="">Artikel 12 des Gesetzes vom 25. Juni 2001 (BGBl. I S. 1215)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="08370088-6105-34c2-8005-0f1a9af0646e"
                      eId="hauptteil-n1_liste-n1_listenelem-n9">
               <akn:num GUID="bb6d99f0-ab2c-3482-a4f4-4c710423b24d"
                        eId="hauptteil-n1_liste-n1_listenelem-n9_bezeichnung-n1">9.</akn:num>
               <akn:p GUID="0293d6dd-6165-3486-b625-c165e9646b41"
                      eId="hauptteil-n1_liste-n1_listenelem-n9_text-n1">den am 1. Januar 2002 in Kraft getretenen<akn:ref GUID="79c36543-35cf-3836-bd8e-d39dfd326c32"
                           eId="hauptteil-n1_liste-n1_listenelem-n9_text-n1_ref-n1"
                           href="">Artikel 6 Nr. 9 des Gesetzes vom 26. November 2001 (BGBl. I S. 3138)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="8d47cfe8-c5c4-34b1-bc34-1abcc982ea43"
                      eId="hauptteil-n1_liste-n1_listenelem-n10">
               <akn:num GUID="2a0d8e4d-8628-3632-9a88-8c6d70ae1c69"
                        eId="hauptteil-n1_liste-n1_listenelem-n10_bezeichnung-n1">10.</akn:num>
               <akn:p GUID="12a995aa-a930-3060-ba5a-c8d58c5904e4"
                      eId="hauptteil-n1_liste-n1_listenelem-n10_text-n1">den am 29. März 2002 in Kraft getretenen<akn:ref GUID="97502ded-0c56-35b8-889c-88a7ca7f6c96"
                           eId="hauptteil-n1_liste-n1_listenelem-n10_text-n1_ref-n1"
                           href="">Artikel 1 des Gesetzes vom 21. März 2002 (BGBl. I S. 1146)</akn:ref>,</akn:p>
            </akn:item>
            <akn:item GUID="3199be56-9ca4-3ca3-8642-17cec90a7cd4"
