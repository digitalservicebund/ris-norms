        <akn:heading GUID="6c522bc6-155a-47fa-b450-02be61624cc8" eId="hauptteil-1_art-2_überschrift-1">Inkrafttreten</akn:heading>
        <akn:paragraph GUID="cc9af166-c2d9-4971-a636-2d918909c62a" eId="hauptteil-1_art-2_abs-1">
          <akn:num GUID="14d359b6-f1ef-4348-932a-fed6e9508d0e" eId="hauptteil-1_art-2_abs-1_bezeichnung-1"/>
