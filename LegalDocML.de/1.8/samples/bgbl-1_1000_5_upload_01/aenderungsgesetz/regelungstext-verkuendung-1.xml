      <akn:body GUID="b5df4017-ad22-41a0-99e8-7bc78880db54" eId="hauptteil-n1">
         <akn:article refersTo="hauptaenderung"
                      GUID="f1b28ef4-846d-4898-adaa-d7b38ae56e67"
                      eId="art-z"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n2">
            <akn:num GUID="aef086b4-bf5c-4e5f-9dc8-35a9d2f74bec" eId="art-z_bezeichnung-n1"/>
            <akn:heading GUID="a046c38e-cf58-4aa8-b809-f61dcd6e0826" eId="art-z_überschrift-n1">Artikel 1</akn:heading>
            <akn:paragraph GUID="7b060db4-4582-49b1-8ded-552749153f16" eId="art-z_abs-z">
               <akn:num GUID="08c1b885-ec9f-4a7b-a4d0-de69f79a7486"
                        eId="art-z_abs-z_bezeichnung-n1"/>
               <akn:list GUID="e10bc729-5fb7-48c8-9ab1-32347baac7d8"
                         eId="art-z_abs-z_untergl-n1">
                  <akn:intro GUID="fb4d6a1f-2f05-46e4-bb4d-ed9d406f6d31"
                             eId="art-z_abs-z_untergl-n1_intro-n1">
                     <akn:p GUID="a975426c-eb0d-4b23-907c-6924243a5539"
                            eId="art-z_abs-z_untergl-n1_intro-n1_text-n1">Das <akn:affectedDocument href="eli/bund/bgbl-1/1001/1/1001-01-01/1/deu/regelungstext-verkuendung-1"
                                              GUID="85a5da0c-c937-43f7-a77f-d7c220fca5e0"
                                              eId="art-z_abs-z_untergl-n1_intro-n1_text-n1_bezugsdoc-n1">Beispielgesetz</akn:affectedDocument> wird wie folgt geändert:</akn:p>
                  </akn:intro>
                  <akn:point GUID="0a88840b-6e17-4f99-87ba-a152498de253"
                             eId="art-z_abs-z_untergl-n1_listenelem-n1">
                     <akn:num GUID="55b7e4f7-37c5-4e8f-ab24-5d45d1256abe"
                              eId="art-z_abs-z_untergl-n1_listenelem-n1_bezeichnung-n1">1.</akn:num>
                     <akn:content GUID="5706fc3d-f6f1-41d3-b8d4-7b9027ddd72a"
                                  eId="art-z_abs-z_untergl-n1_listenelem-n1_inhalt-n1">
                        <akn:p GUID="6c64223a-480d-4426-b6bf-1491b5bbe450"
                               eId="art-z_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1">
                           <akn:mod refersTo="aenderungsbefehl-ersetzen"
                                    GUID="b77a910a-b4fa-4adc-bf75-c37cfccabd3a"
                                    eId="art-z_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1_ändbefehl-n1">In <akn:ref href="eli/bund/bgbl-1/1001/1/1001-01-01/1/deu/regelungstext-verkuendung-1/hauptteil-1_art-1_abs-1_inhalt-1_text-1/29-36.xml"
                                       GUID="2312e3fa-8e2b-4945-9b01-f8a3209d88a6"
                                       eId="art-z_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1_ändbefehl-n1_ref-n1">§ 1 Absatz 1 Satz 1</akn:ref> werden die Wörter <akn:quotedText startQuote="„"
                                              endQuote="“"
                                              GUID="46c8625b-1277-48f4-bd30-00946dfe4d06"
                                              eId="art-z_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1_ändbefehl-n1_quottext-n1">1. Fall</akn:quotedText> durch die Wörter <akn:quotedText startQuote="„"
                                              endQuote="“"
                                              GUID="038e3ef9-38d9-471b-a1ed-4f64c056b5ea"
                                              eId="art-z_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1_ändbefehl-n1_quottext-n2">1. Beispiel</akn:quotedText> ersetzt.</akn:mod>
                        </akn:p>
                     </akn:content>
                  </akn:point>
               </akn:list>
            </akn:paragraph>
         </akn:article>
