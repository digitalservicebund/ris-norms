    <xs:complexType name="liste" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "liste" wird benutzt, um eine Liste mit vordefinierten Strukturelementen einzufügen. Diese verfügt im Gegensatz zu nummerierten und unnummerierten Listen über Listenelemente mit jeweils einer Art- und Zählbezeichnung, einer Überschrift und Text.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="listIntroduction" type="listeEingangssatz" minOccurs="0"/>
         <xs:element name="item" type="listeListenelement" maxOccurs="unbounded"/>
         <xs:element name="listWrapUp" type="listeSchlusssatz" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.liste"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
