   <xs:simpleType name="dateLiterals">
      <xs:annotation>
         <xs:documentation>Ein maschinenlesbares Datum, formatiert als JJJJ-MM-TT. Diese Datumsangabe bezieht sich auf die zum zu referenzierenden Zeitpunkt in der Bundesrepublik Deutschland gültige Zeitzone.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:date"/>
   </xs:simpleType>
   <xs:simpleType name="dateLiterals.ereignisReferenz">
      <xs:annotation>
         <xs:documentation>Beschreibt das Eintrittsdatum eines Ereignisses. Die Datumsangabe bezieht sich auf die zum referenzierten Zeitpunkt in der Bundesrepublik Deutschland gültige Zeitzone.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="dateLiterals"/>
   </xs:simpleType>
   <xs:simpleType name="dateLiterals.expression.FRBRdate">
      <xs:annotation>
         <xs:documentation>Dient der Angabe eines Datums auf der Expression-Ebene. Die Datumsangabe bezieht sich auf die zum referenzierten Zeitpunkt in der Bundesrepublik Deutschland gültige Zeitzone.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="dateLiterals">
         <xs:pattern value="[12][0-9]{3}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[0-1])"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="dateLiterals.manifestation.FRBRdate">
      <xs:annotation>
         <xs:documentation>Dient der Angabe eines Datums auf der Manifestation-Ebene. Die Datumsangabe bezieht sich auf die zum referenzierten Zeitpunkt in der Bundesrepublik Deutschland gültige Zeitzone.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="dateLiterals">
         <xs:pattern value="[12][0-9]{3}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[0-1])"/>
      </xs:restriction>
   </xs:simpleType>
