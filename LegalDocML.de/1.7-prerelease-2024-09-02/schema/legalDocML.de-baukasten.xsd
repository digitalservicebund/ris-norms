         <xs:documentation>Die Klasse "listeSchlusssatz" wird benutzt, um innerhalb einer Liste einen Schlusssatz nach den Listenelementen einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.listeSchlusssatz"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="inhaltsabschnitt" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "inhaltsabschnitt" wird benutzt, um Inhalt in Form eines Abschnitts einzufügen, der neben dem eigentlichen Inhaltselement auch eine Art- und Zählbezeichnung sowie eine Überschrift haben kann.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="num" type="artUndZaehlbezeichnung" minOccurs="0"/>
         <xs:element name="heading" type="ueberschrift" minOccurs="0"/>
         <xs:group ref="inhaltselement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.inhaltsabschnitt"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
