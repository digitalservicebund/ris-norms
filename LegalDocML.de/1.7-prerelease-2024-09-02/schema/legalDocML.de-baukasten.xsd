      <xs:attribute name="class" use="optional" type="classLiterals"/>
      <xs:attribute name="colspan" use="optional" type="colspanLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.th"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="rowspan" use="optional" type="rowspanLiterals"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="tr" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "tr" wird benutzt, um eine Zeile in eine Tabelle einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="th" type="th" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="td" type="td" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="class" use="optional" type="classLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.tr"/>
