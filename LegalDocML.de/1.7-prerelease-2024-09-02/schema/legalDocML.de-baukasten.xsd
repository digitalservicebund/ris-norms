         <xs:documentation>Die Klasse "listeEingangssatz" wird benutzt, um innerhalb einer Liste einen einleitenden Satz vor den Listenelementen einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.listeEingangssatz"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="listeListenelement" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "listeListenelement" wird verwendet, um einen Eintrag in eine Liste einzufügen. Das Listenelement einer Liste besteht aus einer Art- und Zählbezeichnung, einer optionalen Überschrift und kann verschiedene Inhaltselemente benutzen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="num" type="artUndZaehlbezeichnung"/>
         <xs:element name="heading" type="ueberschrift" minOccurs="0"/>
         <xs:group ref="inhaltselement" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.listeListenelement"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="listeSchlusssatz" mixed="true">
