         <xs:documentation>Die Klasse "aenderungZitatStruktur" wird benutzt, um den regelungssprachlichen Teil einer Änderung einzufügen. Dieser wird durch eine zitierte Struktur ausgezeichnet. "aenderungZitatStruktur" wird jedoch nicht für Fragmente eines Inhalts verwendet; sofern nicht der gesamte Inhalt geändert werden soll, sondern nur ein Teil, ist stattdessen "aenderungZitatText" (&lt;akn:quotedText&gt;) zu verwenden.</xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element name="longTitle" type="dokumentenkopfTitel" maxOccurs="unbounded"/>
         <xs:element name="citations" type="ermaechtigungsnormen" maxOccurs="unbounded"/>
         <xs:element name="recitals" type="praeambel" maxOccurs="unbounded"/>
         <xs:group ref="regelungstextGliederung" maxOccurs="unbounded"/>
         <xs:element name="paragraph" type="juristischerAbsatz" maxOccurs="unbounded"/>
         <xs:element name="list"
                     type="juristischerAbsatzUntergliederung"
                     maxOccurs="unbounded"/>
         <xs:element name="recital" type="praeambelInhalt" maxOccurs="unbounded"/>
         <xs:element name="citation" type="ermaechtigungsnorm" maxOccurs="unbounded"/>
         <xs:element name="point" type="elementUntergliederung" maxOccurs="unbounded"/>
         <xs:element name="wrapUp" type="textNachUntergliederung" maxOccurs="unbounded"/>
         <xs:group ref="aenderungsInhalt" maxOccurs="unbounded"/>
         <xs:element name="documentRef" type="anlagenverweis" maxOccurs="unbounded"/>
         <xs:element name="blockContainer"
                     type="verzeichniscontainer"
                     maxOccurs="unbounded"/>
         <xs:element name="componentRef" type="stammformVerweis" maxOccurs="unbounded"/>
      </xs:choice>
      <xs:attribute name="eId" use="required" type="eIdLiterals.aenderungZitatStruktur"/>
      <xs:attribute name="endQuote" use="required" type="endQuoteLiterals"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="startQuote" use="required" type="startQuoteLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="aenderungZitatText" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "aenderungZitatText" wird benutzt, um den regelungssprachlichen Teil einer Änderung einzufügen. Dieser wird durch einen zitierten Text ausgezeichnet. "aenderungZitatText" wird jedoch nur für Fragmente eines Inhalts verwendet; sofern der gesamte Inhalt geändert werden soll, ist stattdessen "aenderungZitatStruktur" (&lt;akn:quotedStructure&gt;) zu verwenden.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.aenderungZitatText"/>
      <xs:attribute name="endQuote" use="required" type="endQuoteLiterals"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="startQuote" use="required" type="startQuoteLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
