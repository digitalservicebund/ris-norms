   <xs:complexType name="gliederungUntertitel" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "gliederungUntertitel" wird genutzt, um ein Gliederungselement einzufügen und damit einen Regelungstext zu untergliedern.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="num" type="artUndZaehlbezeichnung"/>
         <xs:element name="heading" type="ueberschrift"/>
         <xs:group ref="regelungstextGliederung" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.gliederungUntertitel"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:group name="regelungstextGliederung">
      <xs:choice>
         <xs:element name="book"
                     type="gliederungBuch"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="part"
                     type="gliederungTeil"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="chapter"
                     type="gliederungKapitel"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="subchapter"
                     type="gliederungUnterkapitel"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="section"
                     type="gliederungAbschnitt"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="article"
                     type="einzelvorschrift"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="subsection"
                     type="gliederungUnterabschnitt"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="title"
                     type="gliederungTitel"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element name="subtitle"
                     type="gliederungUntertitel"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="regelungstextHauptteil" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "regelungstextHauptteil" wird benutzt, um innerhalb eines Regelungstextes einen Hauptteil einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="regelungstextGliederung"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.regelungstextHauptteil"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="ortUndDatum" mixed="true">
      <xs:annotation>
         <xs:documentation>Ort und Datum</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="location" type="ort"/>
         <xs:element name="date" type="datum"/>
      </xs:sequence>
      <xs:attribute name="class" use="optional" type="classLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.ortUndDatum"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="signatur" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "signatur" wird verwendet, um eine Signatur für die Verkündungsfassung eines Regelungstextes hinzuzufügen.</xs:documentation>
      </xs:annotation>
      <xs:all>
         <xs:element name="person" type="personenname" minOccurs="0"/>
         <xs:element name="role" type="funktionsbezeichnung" minOccurs="0"/>
         <xs:element name="organization" type="organisation" minOccurs="0"/>
      </xs:all>
      <xs:attribute name="eId" use="required" type="eIdLiterals.signatur"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="signaturblock" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "signaturblock" wird genutzt, um eine oder mehrere Signaturen anzugeben.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="p" type="ortUndDatum"/>
         <xs:element name="signature" type="signatur" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.signaturblock"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
   </xs:complexType>
   <xs:complexType name="schlussformel" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "schlussformel" wird benutzt, um die Schlussformel eines Regelungstextes einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inhaltselementOhneAN" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.schlussformel"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="name" use="required" type="nameLiterals.schlussformel"/>
      <xs:attribute name="refersTo"
                    use="required"
                    type="refersToLiterals.schlussformel"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="regelungstextSchluss" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "regelungstextSchluss" wird benutzt, um den Schlussteil eines Regelungstextes einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="formula" type="schlussformel" minOccurs="0"/>
         <xs:element name="blockContainer" type="signaturblock" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.regelungstextSchluss"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="datumContainer" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "datumContainer" wird genutzt, um innerhalb eines Regelungstextes ein Strukturelement für die Datumsangabe einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="date" type="datum"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.datumContainer"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="name" use="required" type="nameLiterals.datumContainer"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="regelungstextDokumentenkopf" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "regelungstextDokumentenkopf" wird benutzt, um einem Regelungstext einen Dokumentenkopf hinzuzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="longTitle" type="dokumentenkopfTitel"/>
         <xs:element name="block" type="datumContainer" minOccurs="0"/>
         <xs:element name="container"
                     type="regelungstextDokumentenkopfContainer"
                     minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="eId"
                    use="required"
                    type="eIdLiterals.regelungstextDokumentenkopf"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="regelungstextDokumentenkopfContainer" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "regelungstextDokumentenkopfContainer" wird benutzt, um innerhalb eines Dokumentenkopfes eines Regelungstextes ein Container-Element hinzuzufügen. Diese Klasse wird nur optional für Bestandsrecht verwendet.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inhaltselement" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="xml:id" use="optional"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="eId"
                    use="required"
                    type="eIdLiterals.regelungstextDokumentenkopfContainer"/>
   </xs:complexType>
   <xs:complexType name="eingangsformel" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "eingangsformel" wird benutzt, um die Eingangsformel eines Dokumentes zu beschreiben. Bei Verordnungen und Verwaltungsvorschriften wird die Eingangsformel nicht verwendet.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inhaltselementOhneAN" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.eingangsformel"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="name" use="required" type="nameLiterals.eingangsformel"/>
      <xs:attribute name="refersTo"
                    use="required"
                    type="refersToLiterals.eingangsformel"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="ermaechtigungsnorm" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "ermaechtigungsnorm" wird benutzt, um eine Ermächtigungsnorm mit einer Art- und Zählbezeichnung, einer Überschrift und einem textuellen Inhalt hinzuzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="num" type="artUndZaehlbezeichnung" minOccurs="0"/>
         <xs:element name="heading" type="ueberschrift" minOccurs="0"/>
         <xs:group ref="HTMLblock" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.ermaechtigungsnorm"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="ermaechtigungsnormEingangssatz" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "ermaechtigungsnormEingangssatz" wird benutzt, um innerhalb von Ermächtigungsnormen einen Eingangssatz einzufügen, welcher für jede einzelne Ermächtigungsnorm gilt.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="p" type="p"/>
      </xs:sequence>
      <xs:attribute name="eId"
                    use="required"
                    type="eIdLiterals.ermaechtigungsnormEingangssatz"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="ermaechtigungsnormen" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "ermaechtigungsnormen" wird verwendet, um Ermächtigungsnormen für Verordnungen, Verwaltungsvorschriften und Satzungen hinzuzufügen. Diese sind für Verordnungen verpflichtend und werden mit Schematron-Regeln überprüft.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="num" type="artUndZaehlbezeichnung" minOccurs="0"/>
         <xs:element name="heading" type="ueberschrift" minOccurs="0"/>
         <xs:element name="intro" type="ermaechtigungsnormEingangssatz" minOccurs="0"/>
         <xs:element name="citation" type="ermaechtigungsnorm" maxOccurs="unbounded"/>
         <xs:element name="wrapUp" type="ermaechtigungsnormSchlusssatz" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.ermaechtigungsnormen"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="ermaechtigungsnormSchlusssatz" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "ermaechtigungsnormSchlusssatz" wird benutzt, um innerhalb von Ermächtigungsnormen einen Schlusssatz einzufügen, welcher für jede einzelne Ermächtigungsnorm gilt.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="p" type="p"/>
      </xs:sequence>
      <xs:attribute name="eId"
                    use="required"
                    type="eIdLiterals.ermaechtigungsnormSchlusssatz"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="praeambel" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "praeambel" wird benutzt, um innerhalb eines Regelungstextes eine optionale Präambel einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="num" type="artUndZaehlbezeichnung" minOccurs="0"/>
         <xs:element name="heading" type="ueberschrift" minOccurs="0"/>
         <xs:element name="recital" type="praeambelInhalt" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.praeambel"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="praeambelInhalt" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "praeambelInhalt" wird benutzt, um den Inhalt einer Präambel darzustellen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inhaltselementOhneAN" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.praeambelInhalt"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="eingangsformelUndVerzeichnis" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "eingangsformelUndVerzeichnis" wird benutzt, um innerhalb eines Regelungstextes eine Eingangsformel, Ermächtigungsnormen oder Präambeln, sowie Verzeichnisse einzufügen. Durch Schematron-Regeln wird überprüft, dass Eingangsformeln nur bei Gesetzen benutzt werden und dass Verordnungen und Verwaltungsvorschriften Ermächtigungsnormen bereitstellen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="formula" type="eingangsformel" minOccurs="0"/>
         <xs:element name="citations" type="ermaechtigungsnormen" minOccurs="0"/>
         <xs:element name="recitals" type="praeambel" minOccurs="0"/>
         <xs:element name="blockContainer"
                     type="verzeichniscontainer"
                     minOccurs="0"
                     maxOccurs="2"/>
      </xs:sequence>
      <xs:attribute name="eId"
                    use="required"
                    type="eIdLiterals.eingangsformelUndVerzeichnis"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="verzeichniscontainer" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "verzeichniscontainer" wird genutzt, um innerhalb eines Regelungstextes ein Strukturelement für Verzeichnisse beizufügen. Die Darstellung spezifischer Verzeichniscontainer erfolgt ausschließlich durch die Zuordnung von @refersTo-Attributen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="heading" type="ueberschrift"/>
         <xs:element name="toc" type="inhaltsuebersicht"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.verzeichniscontainer"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo"
                    use="required"
                    type="refersToLiterals.verzeichniscontainer"/>
   </xs:complexType>
   <xs:group name="regelungstextEinleitung">
      <xs:sequence>
         <xs:element name="preface" type="regelungstextDokumentenkopf"/>
         <xs:element name="preamble" type="eingangsformelUndVerzeichnis" minOccurs="0"/>
      </xs:sequence>
   </xs:group>
   <xs:complexType name="teildokument" mixed="false">
      <xs:annotation>
         <xs:documentation/>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="documentRef" type="teildokumentVerweis"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.teildokument"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="teildokumente" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse “teildokumente” wird benutzt, um innerhalb eines Dokuments einen Verweis auf ein Teildokument welches zu dem Dokument gehört einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="component" type="teildokument" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.teildokumente"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
   </xs:complexType>
   <xs:complexType name="teildokumentVerweis" mixed="false">
      <xs:annotation>
         <xs:documentation/>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.teildokumentVerweis"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="href" use="required" type="hrefLiterals"/>
      <xs:attribute name="showAs"
                    use="required"
                    type="showAsLiterals.teildokumentVerweis"/>
   </xs:complexType>
   <xs:complexType name="artUndZaehlbezeichnung" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "artUndZaehlbezeichnung" wird benutzt, um die verschiedenen Inhalte eines Regelungstexts (z. B. Gliederungsebenen, oder Artikel und Paragraphen) zu strukturieren.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.artUndZaehlbezeichnung"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="externesMarkup" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "externesMarkup" wird benutzt, um die Einbindung von Elementen aus externen Schemata in LegalDocML.de zu erlauben, z. B. für Formeln aus der Math-Library.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.externesMarkup"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
   </xs:complexType>
   <xs:complexType name="ueberschrift" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "ueberschrift" wird verwendet, um eine Überschrift einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.ueberschrift"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
</xs:schema>
