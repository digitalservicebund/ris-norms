      </xs:choice>
   </xs:group>
   <xs:complexType name="aenderungsbefehl" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "aenderungsbefehl" wird in einem Regelungstext benutzt, um eine an einem Rechtsakt vorzunehmende Änderung auszuzeichnen. Auf Ebene des Schemas werden keine spezifischen Arten von Änderungsbefehlen unterschieden, da diese strukturell identisch aufgebaut sind. Die Darstellung spezifischer Änderungsbefehle erfolgt ausschließlich durch die Zuordnung von @refersTo-Attributen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="aenderungsbefehlInline" minOccurs="0" maxOccurs="unbounded"/>
         <xs:group ref="regelungssprachlicherTeil" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.aenderungsbefehl"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo"
                    use="optional"
                    type="refersToLiterals.aenderungsbefehl"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:group name="aenderungsbefehlInline">
      <xs:choice>
         <xs:element name="ref" type="referenz" maxOccurs="unbounded"/>
         <xs:element name="rref" type="bereichsreferenz" maxOccurs="unbounded"/>
         <xs:group ref="HTMLinline" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element name="affectedDocument"
                     type="betroffenesDokument"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="aenderungZitatStruktur" mixed="false">
