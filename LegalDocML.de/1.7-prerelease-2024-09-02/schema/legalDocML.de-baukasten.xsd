   <xs:group name="ANinline">
      <xs:choice>
         <xs:element name="mod" type="aenderungsbefehl"/>
         <xs:element name="ref" type="referenz"/>
         <xs:element name="rref" type="bereichsreferenz"/>
         <xs:element name="authorialNote" type="fussnote"/>
         <xs:element name="mmod" type="kombinierterAenderungsbefehl"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="fussnote" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "fussnote" wird benutzt, um eine Fußnote in einem beliebigen Inhaltselement einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="HTMLblock" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.fussnote"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="marker" use="required" type="markerLiterals"/>
      <xs:attribute name="placement" use="required" type="placementLiterals.fussnote"/>
      <xs:attribute name="placementBase" use="required" type="placementBaseLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="referenz" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "referenz" wird verwendet, um einen Verweis auf ein dokumentinternes Element oder auf ein Element eines externen Dokuments einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.referenz"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="href" use="required" type="hrefLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="bereichsreferenz" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "bereichsreferenz" wird verwendet, um eine Bereichsreferenz auf fortlaufende dokumentinterne Elemente oder auf fortlaufende Elements eines externen Dokuments einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.bereichsreferenz"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="from" use="required" type="fromLiterals"/>
      <xs:attribute name="upTo" use="required" type="upToLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="a" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "a" ist sinngemäß aus HTML übernommen und markiert Text, der als Link fungieren soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="formatierteInlineelemente" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.a"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="href" use="required" type="hrefLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="abbr" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "abbr" ist sinngemäß aus HTML übernommen und markiert eine Abkürzung als solche. Das ausgeschriebene Wort kann als Attribut angegeben werden.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="formatierteInlineelemente" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.abbr"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="b" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "b" ist sinngemäß aus HTML übernommen und markiert Text, der fett (bold) dargestellt werden soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="formatierteInlineelemente" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.b"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:group name="formatierteInlineelemente">
      <xs:choice>
         <xs:element name="docNumber" type="drucksachennummer"/>
         <xs:element name="organization" type="organisation"/>
         <xs:group ref="HTMLinline"/>
      </xs:choice>
   </xs:group>
   <xs:group name="HTMLinline">
      <xs:choice>
         <xs:element name="span" type="span"/>
         <xs:element name="b" type="b"/>
         <xs:element name="i" type="i"/>
         <xs:element name="a" type="a"/>
         <xs:element name="u" type="u"/>
         <xs:element name="sub" type="sub"/>
         <xs:element name="sup" type="sup"/>
         <xs:element name="abbr" type="abbr"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="i" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "i" ist sinngemäß aus HTML übernommen und markiert Text, der kursiv (italic) dargestellt werden soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="formatierteInlineelemente" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.i"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="span" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "span" ist sinngemäß aus HTML übernommen und wird für generische Inlineelemente genutzt.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="formatierteInlineelemente" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="class" use="optional" type="classLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.span"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals.span"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="sub" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "sub" ist sinngemäß aus HTML übernommen und markiert Text, der tiefgestellt (subscript) dargestellt werden soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="formatierteInlineelemente" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.sub"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="sup" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "sup" ist sinngemäß aus HTML übernommen und markiert Text, der hochgestellt (superscript) dargestellt werden soll. Innerhalb dieser Klasse können Fußnoten eingefügt werden.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="formatierteInlineelemente" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.sup"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals.sup"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="u" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "u" ist sinngemäß aus HTML übernommen und markiert Text, der unterstrichen (underline) dargestellt werden soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="formatierteInlineelemente" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.u"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="datum" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "datum" wird genutzt, um Datumsangaben zu einem Dokument innerhalb eines Rechtsetzungsdokuments einfügen zu können. Auf Ebene des Schemas werden keine spezifischen Arten von einem Datum unterschieden, da diese strukturell identisch aufgebaut sind. Die Darstellung spezifischer Arten von einem Datum erfolgt ausschließlich durch die Zuordnung von @refersTo-Attributen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="date" use="required" type="dateLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.datum"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals.datum"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="funktionsbezeichnung" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "funktionsbezeichnung" wird benutzt, um die Funktion der in einem Anschreiben genannten Person, oder in einer Signatur genannten Person in einem nicht-normativen Dokument zu bezeichnen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.funktionsbezeichnung"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo"
                    use="required"
                    type="refersToLiterals.funktionsbezeichnung"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:group name="kontextelement">
      <xs:choice>
         <xs:element name="date" type="datum"/>
         <xs:element name="location" type="ort"/>
         <xs:element name="organization" type="organisation"/>
         <xs:element name="person" type="personenname"/>
         <xs:element name="role" type="funktionsbezeichnung"/>
         <xs:element name="signature" type="signaturInhalt"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="organisation" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "organisation" wird verwendet, um eine Organisiation einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.organisation"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals.organisation"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute name="title" use="required" type="titleLiterals.organisation"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="ort" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "ort" wird verwendet, um eine Ortsangabe einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.ort"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals.ort"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="personenname" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "personenname" wird verwendet, um den Namen einer Person einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.personenname"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals.personenname"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="signaturInhalt" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "signaturInhalt" bündelt mehrere Elemente, die für eine Signatur relevant sind, darunter fallen Ort, Datum, Organisation, Funktionsbezeichnung und Personenname. Die Klasse "signaturInhalt" wird nur bei nicht-normativen Dokumenten verwendet.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="location" type="ort" minOccurs="0"/>
         <xs:element name="date" type="datum" minOccurs="0"/>
         <xs:element name="organization" type="organisation" minOccurs="0"/>
         <xs:element name="role" type="funktionsbezeichnung" minOccurs="0"/>
         <xs:element name="person" type="personenname" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.signaturInhalt"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="dokumentenStatus" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "dokumentenStatus" wird benutzt, um den Prozessstatus eines Rechtsetzungsdokuments in der Entwurfsfassung anzugeben (z. B. Referentenentwurf, Regierungsentwurf ect.).</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.dokumentenStatus"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="dokumententitel" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "dokumententitel" wird benutzt, um den Titel eines Dokuments anzugeben. Dies kann beispielsweise der Titel des Rechtsetzungsdokuments insgesamt sein oder der Titel eines Teildokuments des Rechtsetzungsdokuments.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.dokumententitel"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="dokumentenTyp" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "dokumentenTyp" wird genutzt, um den Typ des Rechtsetzungsdokuments zu definieren (z. B. Regelungsentwurf eines Gesetzes).</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.dokumentenTyp"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="drucksachennummer" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "drucksachennummer" wird benutzt, um eine Drucksachennummer in den Dokumentenkopf eines Teildokuments eines Rechtsetzungsdokuments einzufügen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.drucksachennummer"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo"
                    use="optional"
                    type="refersToLiterals.drucksachennummer"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="initiant" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "initiant" kennzeichnet die Initiantinnen / die Initianten eines Rechtsetzungsdokuments.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.initiant"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="kurztitel" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "kurztitel" wird benutzt, um den Kurztitel eines Regelungstextes anzugeben.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.kurztitel"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="refersTo" use="optional" type="refersToLiterals.kurztitel"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:group name="titelelement">
      <xs:choice>
         <xs:element name="docTitle" type="dokumententitel"/>
         <xs:element name="shortTitle" type="kurztitel"/>
         <xs:element name="docStage" type="dokumentenStatus"/>
         <xs:element name="docProponent" type="initiant"/>
         <xs:element name="docType" type="dokumentenTyp"/>
         <xs:element name="docNumber" type="drucksachennummer"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="betroffenesDokument" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "betroffenesDokument" wird genutzt, um das von einer Änderung betroffene Dokument anzugeben.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.betroffenesDokument"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="href" use="required" type="hrefLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="bezugsdokument" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse “bezugsdokument” wird verwendet, um auf ein Dokument Bezug zu nehmen.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.bezugsdokument"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="href" use="required" type="hrefLiterals"/>
      <xs:attribute name="style" use="optional" type="styleLiterals"/>
   </xs:complexType>
   <xs:complexType name="inline" mixed="true">
      <xs:annotation>
         <xs:documentation>Die Klasse "inline" bündelt Elemente, die nicht in LegalDocML spezifiziert sind, jedoch für das deutsche Anwendungsprofil LegalDocML. de benötigt werden, um zusätzliche Inhalte eines Dokuments auszuzeichnen. Die Bündelung durch die Klasse "inlineelement" beruht ausschließlich auf modellierungstechnischen Erwägungen (z. B. Amtliche Abkürzung).</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="inlineelement" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="eId" use="required" type="eIdLiterals.inline"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="name" use="required" type="nameLiterals.inline"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals.inline"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="marker" mixed="false">
      <xs:annotation>
         <xs:documentation>Mit der Klasse "marker" wird eine Stelle im Text semantisch markiert. Die konkrete Art der Markierung wird über das Attribut refersTo angegeben.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="name" use="optional" type="nameLiterals.marker"/>
      <xs:attribute name="refersTo" use="required" type="refersToLiterals.marker"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.marker"/>
   </xs:complexType>
   <xs:group name="ANmarker">
      <xs:choice>
         <xs:element name="eop" type="endeSeite"/>
         <xs:element name="eol" type="endeZeile"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="endeSeite" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "endeSeite" markiert einen Seitenumbruch. Über die Attribute kann dieser spezifischer ausgestaltet werden, z. B. falls die Seite in einem Wort umbrechen soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="breakAt" use="optional" type="breakAtLiterals"/>
      <xs:attribute name="breakWith" use="optional" type="breakWithLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.endeSeite"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="number" use="optional" type="numberLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="endeZeile" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "endeZeile" markiert einen Zeilenumbruch. Über die Attribute kann dieser spezifischer ausgestaltet werden, z. B. falls die Zeile in einem Wort umbrechen soll.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="breakAt" use="optional" type="breakAtLiterals"/>
      <xs:attribute name="breakWith" use="optional" type="breakWithLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.endeZeile"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="number" use="optional" type="numberLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:complexType name="br" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "br" ist sinngemäß aus HTML übernommen und markiert einen Zeilenumbruch.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.br"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:group name="HTMLmarker">
      <xs:choice>
         <xs:element name="img" type="img"/>
         <xs:element name="br" type="br"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="img" mixed="false">
      <xs:annotation>
         <xs:documentation>Die Klasse "img" ist sinngemäß aus HTML übernommen und wird zum Einbinden von Bildern genutzt.</xs:documentation>
      </xs:annotation>
      <xs:sequence/>
      <xs:attribute name="alt" use="optional" type="altLiterals.img"/>
      <xs:attribute name="class" use="optional" type="classLiterals"/>
      <xs:attribute name="eId" use="required" type="eIdLiterals.img"/>
      <xs:attribute name="GUID" use="required" type="GUIDLiterals"/>
      <xs:attribute name="height" use="optional" type="heightLiterals"/>
      <xs:attribute name="src" use="required" type="srcLiterals"/>
      <xs:attribute name="title" use="optional" type="titleLiterals.img"/>
      <xs:attribute name="width" use="optional" type="widthLiterals"/>
      <xs:attribute ref="xml:id" use="optional"/>
   </xs:complexType>
   <xs:group name="inlineelement">
      <xs:choice>
         <xs:group ref="ANinline"/>
         <xs:group ref="HTMLinline"/>
         <xs:group ref="kontextelement"/>
         <xs:group ref="titelelement"/>
         <xs:element name="inline" type="inline"/>
         <xs:group ref="HTMLmarker"/>
         <xs:group ref="ANmarker"/>
         <xs:element name="marker" type="marker"/>
         <xs:element name="affectedDocument" type="betroffenesDokument"/>
         <xs:element name="relatedDocument" type="bezugsdokument"/>
         <xs:element name="session" type="sitzung"/>
