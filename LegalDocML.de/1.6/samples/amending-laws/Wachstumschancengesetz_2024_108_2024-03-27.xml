        <akn:body eId="hauptteil-1" GUID="ce0ac10d-510e-4148-8a8d-a4406a8b0a67">
            <!-- §1 -->
            <akn:article eId="hauptteil-1_art-1" GUID="17e9c25e-251d-48a7-8e74-3f0bb4b93b6b" period="#geltungszeitgr-2" refersTo="hauptaenderung">
                <akn:num eId="hauptteil-1_art-1_bezeichnung-1" GUID="24f5f556-e005-4d10-8d7e-d92f141780d7">
