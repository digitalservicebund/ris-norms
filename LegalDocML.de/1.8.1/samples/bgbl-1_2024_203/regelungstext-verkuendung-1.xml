      <akn:preamble GUID="f1fae953-1959-444c-b191-d6e49e757919" eId="präambel-n1">
         <akn:citations GUID="f09c5f7d-f539-45b5-962d-b8ac3844daae"
                        eId="präambel-n1_ernormen-n1">
            <akn:citation GUID="b5e2c9c6-231f-4a40-965f-ec17befdcee6"
                          eId="präambel-n1_ernormen-n1_ernorm-n1">
               <akn:p GUID="e17e5fc3-7b2c-4a39-b685-b23b348f878c"
                      eId="präambel-n1_ernormen-n1_ernorm-n1_text-n1">Auf Grund des § 753 Absatz 3, des § 758a Absatz 6 Satz 1 und 3 und des § 829 Absatz 4 Satz 1 und 3 der Zivilprozessordnung, von denen § 753 Absatz 3 zuletzt durch Artikel 1 Nummer 4 Buchstabe b des Gesetzes vom 21. November 2016 (BGBl. I S. 2591) geändert worden ist und § 758a Absatz 6 Satz 1 und § 829 Absatz 4 Satz 1 durch Artikel 145 Nummer 4 der Verordnung vom 31. August 2015 (BGBl. I S. 1474) geändert worden sind, in Verbindung mit § 1 Absatz 2 des Zuständigkeitsanpassungsgesetzes vom 16. August 2002 (BGBl. I S. 3165) und mit dem Organisationserlass vom 8. Dezember 2021 (BGBl. I S. 5176) verordnet das Bundesministerium der Justiz:</akn:p>
            </akn:citation>
         </akn:citations>
      </akn:preamble>
      <akn:body GUID="2365c58a-7989-4bcc-880b-aa4bb07e9a30" eId="hauptteil-n1">
         <akn:article GUID="25f6037a-26c6-47d6-b969-b701c37e9a16"
                      eId="art-z1"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1"
                      refersTo="hauptaenderung">
            <akn:num GUID="7c983fd8-052a-48a6-b1cf-24aa976346fa" eId="art-z1_bezeichnung-n1">Artikel 1</akn:num>
            <akn:heading GUID="1a1ae662-90a0-42a8-9666-1be10367a946" eId="art-z1_überschrift-n1">Änderung der Zwangsvollstreckungsformular-Verordnung</akn:heading>
            <akn:paragraph GUID="6c8b3206-4c3f-4144-aed5-7630a2958a6c" eId="art-z1_abs-z">
               <akn:num GUID="16556b49-9279-4160-bf60-f83a923994c3"
                        eId="art-z1_abs-z_bezeichnung-n1"/>
               <akn:content GUID="1b97a747-88c7-4df0-8db5-70e73833c01e"
                            eId="art-z1_abs-z_inhalt-n1">
                  <akn:blockList GUID="c2c7f24e-9f14-4c0a-8618-0775a99defc6"
                                 eId="art-z1_abs-z_inhalt-n1_liste-n1">
                     <akn:listIntroduction GUID="4f8ad690-712c-4d9b-a217-96cc45c6e2bf"
                                           eId="art-z1_abs-z_inhalt-n1_liste-n1_listeneing-n1">
                     Die Zwangsvollstreckungsformular-Verordnung vom 16. Dezember 2022 (BGBl. I S. 2368), die durch Artikel 1 der Verordnung vom 24. November 2023 (BGBl. 2023 I Nr. 320) geändert worden ist, wird wie folgt geändert:
                  </akn:listIntroduction>
                     <akn:item GUID="03bf1973-5228-4ee1-bd69-4b16d036bac9"
                               eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1">
                        <akn:num GUID="c2827068-ec3e-475a-9ac6-9eea7b2a9678"
                                 eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_bezeichnung-n1">1.</akn:num>
                        <akn:p GUID="d38eafb8-116b-4c1b-85e5-204710deafc7"
                               eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1">
                           <akn:mod GUID="dde7933b-32da-4990-96e8-c73e8a0a41ec"
                                    eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1"
                                    refersTo="aenderungsbefehl-neufassung">§ 6 wird wie folgt gefasst: <akn:quotedStructure GUID="a0ea7123-58e2-4b7b-99fe-411c8c1c8f5f"
                                                   eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1"
                                                   endQuote="“"
                                                   startQuote="„">
                                 <akn:article GUID="54c6147c-f50b-4fee-a237-e158b471a4c6"
                                              eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6"
                                              period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1"
                                              refersTo="stammform">
                                    <akn:num GUID="99ab1283-2e8e-4383-a3aa-67df496b0195"
                                             eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6_bezeichnung-n1">§ 6</akn:num>
                                    <akn:heading GUID="cd36af73-2af4-4029-abeb-4109a6aeb010"
                                                 eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6_überschrift-n1">Übergangsregelung</akn:heading>
                                    <akn:paragraph GUID="41225d07-8aa5-4da2-b46a-9ae365511e91"
                                                   eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6_abs-z1">
                                       <akn:num GUID="48112be5-52a1-4d0f-bfc4-88cb85fe473b"
                                                eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6_abs-z1_bezeichnung-n1">(1)</akn:num>
                                       <akn:content GUID="d212b4c5-bf61-4ba1-9c61-f507b780686c"
                                                    eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6_abs-z1_inhalt-n1">
                                          <akn:blockList GUID="802daef0-ac1a-46fa-b536-a53c26ad5171"
                                                         eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6_abs-z1_inhalt-n1_liste-n1">
                                             <akn:listIntroduction GUID="d99ea42f-368e-483e-9a88-c9eeeab51a8b"
                                                                   eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6_abs-z1_inhalt-n1_liste-n1_listeneing-n1">
                                             Für bis einschließlich 30. September 2025 gestellte
                                          </akn:listIntroduction>
                                             <akn:item GUID="e191ea00-1f1a-404e-a05d-a7b470c43edc"
                                                       eId="art-z1_abs-z_inhalt-n1_liste-n1_listenelem-n1_text-n1_ändbefehl-n1_quotstruct-n1_art-z6_abs-z1_inhalt-n1_liste-n1_listenelem-n1">
                                                <akn:num GUID="fa165127-2ed4-4c45-bdcd-e9cd352c001b"
