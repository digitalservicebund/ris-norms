        <akn:paragraph GUID="f6576a7a-241c-4b40-8289-f9dfd116870b" eId="hauptteil-1_art-2_abs-1">
          <akn:num GUID="5b19faf5-2d09-4b3c-a0a7-17f9650746dc" eId="hauptteil-1_art-2_abs-1_bezeichnung-1"/>
          <akn:content GUID="f5662038-ead2-4497-805c-245881ae4a69" eId="hauptteil-1_art-2_abs-1_inhalt-1">
            <akn:p GUID="909f4962-5953-47b9-baac-915224e71853" eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1">Dieses Gesetz tritt am <akn:date date="1000-03-01" refersTo="inkrafttreten-datum" GUID="b428bdfd-82aa-42b8-a439-a9551b4ec548" eId="hauptteil-1_art-2_abs-1_inhalt-1_text-1_datum-1">06.01.1000</akn:date> in Kraft.</akn:p>
          </akn:content>
        </akn:paragraph>
      </akn:article>
