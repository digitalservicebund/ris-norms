        <akn:heading GUID="078c9b53-192a-4f8c-9bc8-cb520210db78" eId="hauptteil-1_art-1_überschrift-1">Artikel 1</akn:heading>
        <akn:paragraph GUID="f567756c-f125-4399-a6d3-e3b1da8e13a4" eId="hauptteil-1_art-1_abs-1">
          <akn:num GUID="12439711-1f13-46a1-b5e4-9494b8e4ea68" eId="hauptteil-1_art-1_abs-1_bezeichnung-1"/>
          <akn:list GUID="b5233f08-0fd4-4b46-9288-349228d58222" eId="hauptteil-1_art-1_abs-1_untergl-1">
            <akn:intro GUID="3be8225f-9716-4f80-a9ca-5f108c1500d2" eId="hauptteil-1_art-1_abs-1_untergl-1_intro-1">
              <akn:p GUID="cba86b14-fb9b-4f14-b919-6a43d67d6fa1" eId="hauptteil-1_art-1_abs-1_untergl-1_intro-1_text-1">Das <akn:affectedDocument href="eli/bund/bgbl-1/1001/1/1001-01-01/1/deu/regelungstext-1" GUID="f565717a-2c62-49d6-a30c-0f93ec236645" eId="hauptteil-1_art-1_abs-1_untergl-1_intro-1_text-1_bezugsdoc-1">Beispielgesetz</akn:affectedDocument> wird wie folgt geändert:</akn:p>
            </akn:intro>
            <akn:point GUID="ab7b0ba9-c53c-4767-8661-511b3ae40ab8" eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1">
              <akn:num GUID="58bbdf81-7d76-41f0-a4c9-3573d4232268" eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_bezeichnung-1">1.</akn:num>
              <akn:content GUID="39b76e70-5a20-46af-89ff-035c9f34d657" eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1">
                <akn:p GUID="06d5bbe9-8034-4bc2-a007-fa6642f6eff6" eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1">
                  <akn:mod refersTo="aenderungsbefehl-ersetzen" GUID="5bfb6101-c201-460d-b294-2f2c70d1a913" eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1">In <akn:ref href="eli/bund/bgbl-1/1001/1/1001-01-01/1/deu/regelungstext-1/hauptteil-1_art-1_abs-1_inhalt-1_text-1/29-36.xml" GUID="426e9628-ae42-40d5-bf7e-d02f06300a20" eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_ref-1">§ 1 Absatz 1 Satz 1</akn:ref> werden die Wörter <akn:quotedText startQuote="„" endQuote="“" GUID="a0682148-e54d-44ef-83f8-c088848aa450" eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quottext-1">1. Fall</akn:quotedText> durch die Wörter <akn:quotedText startQuote="„" endQuote="“" GUID="d0b00531-44bc-4a73-9f02-e6f9a81a2ca7" eId="hauptteil-1_art-1_abs-1_untergl-1_listenelem-1_inhalt-1_text-1_ändbefehl-1_quottext-2">1. Beispiel</akn:quotedText> ersetzt.</akn:mod>
                </akn:p>
              </akn:content>
            </akn:point>
          </akn:list>
        </akn:paragraph>
