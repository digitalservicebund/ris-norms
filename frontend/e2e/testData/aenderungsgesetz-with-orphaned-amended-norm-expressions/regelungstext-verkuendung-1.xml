      <akn:body GUID="539ed544-5884-48fc-b9b8-693c77dd130a" eId="hauptteil-n1">
         <akn:article refersTo="hauptaenderung"
                      GUID="5f40f517-84b9-4d65-93ae-574ea315ae59"
                      eId="art-z1"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1">
            <akn:num GUID="f5ccb540-db42-44d5-8a28-1d89f4ce5378" eId="art-z1_bezeichnung-n1"> Artikel 1</akn:num>
            <akn:heading GUID="bcfc76e0-4946-45ac-8fe8-d245657330b8" eId="art-z1_überschrift-n1">Änderung des Vereinsgesetzes</akn:heading>
            <!-- Absatz (1) -->
            <akn:paragraph GUID="aa308bf0-5080-47aa-9e25-8af81e533b7c" eId="art-z1_abs-z">
               <akn:num GUID="bde8599b-82b4-4c42-ae99-6738d7fcbbd6"
                        eId="art-z1_abs-z_bezeichnung-n1"/>
               <akn:list GUID="b7cf8095-bf95-4079-b145-1f2434a94501"
                         eId="art-z1_abs-z_untergl-n1">
                  <akn:intro GUID="345666af-e6a9-4a10-98ff-38c2ac0f1ae4"
                             eId="art-z1_abs-z_untergl-n1_intro-n1">
                     <akn:p GUID="27571807-7413-4b1b-ba53-59bdd6a87193"
                            eId="art-z1_abs-z_untergl-n1_intro-n1_text-n1"> Das <akn:affectedDocument href="eli/bund/bgbl-1/1964/s593/1964-08-05/1/deu/regelungstext-1"
                                              GUID="48d3972a-7e55-46dc-b193-3d376aa085f0"
                                              eId="art-z1_abs-z_untergl-n1_intro-n1_text-n1_bezugsdoc-n1">Vereinsgesetz vom 5. August 1964 (BGBl. I S. 593), das zuletzt durch … geändert worden ist</akn:affectedDocument>, wird wie folgt geändert:</akn:p>
                  </akn:intro>
                  <!-- Nummer 1 wurde entfernt -->
                  <!-- Nummer 2 -->
                  <akn:point GUID="688f1c3d-67e3-4d52-9f39-2a165c90d9c0"
                             eId="art-z1_abs-z_untergl-n1_listenelem-n1">
                     <akn:num GUID="0c463816-2ac4-4b72-8da5-1cd770ab7f38"
                              eId="art-z1_abs-z_untergl-n1_listenelem-n1_bezeichnung-n1"> 2.</akn:num>
                     <akn:content GUID="a46edbe2-fdbc-4144-948b-0f1661084eb9"
                                  eId="art-z1_abs-z_untergl-n1_listenelem-n1_inhalt-n1">
                        <akn:p GUID="affeab24-4248-440f-8c40-3836c8190d23"
                               eId="art-z1_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1">
                           <akn:mod refersTo="aenderungsbefehl-ersetzen"
                                    GUID="9b5361b5-4405-4951-82dc-b9a34a4c77cc"
                                    eId="art-z1_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1_ändbefehl-n1">In <akn:ref href="eli/bund/bgbl-1/1964/s593/1964-08-05/1/deu/regelungstext-1/hauptteil-1_art-1_abs-1_untergl-1_listenelem-2_inhalt-1_text-1/9-34.xml"
                                       GUID="6969b681-968a-419e-a743-480ded369a19"
                                       eId="art-z1_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1_ändbefehl-n1_ref-n1">§ 20 Absatz 1 Satz 2</akn:ref> wird die Angabe <akn:quotedText startQuote="„"
                                              endQuote="“"
                                              GUID="0331f81c-2426-44a3-9d75-3c5b20319f4b"
                                              eId="art-z1_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1_ändbefehl-n1_quottext-n1">§ 9 Abs. 1 Satz 2, Abs. 2</akn:quotedText> durch die Wörter <akn:quotedText startQuote="„"
                                              endQuote="“"
                                              GUID="818116c7-0dca-455c-99b6-f509416356cb"
                                              eId="art-z1_abs-z_untergl-n1_listenelem-n1_inhalt-n1_text-n1_ändbefehl-n1_quottext-n2">§ 9 Absatz 1 Satz 2, Absatz 2 oder 3</akn:quotedText> ersetzt.</akn:mod>
                        </akn:p>
                     </akn:content>
                  </akn:point>
               </akn:list>
            </akn:paragraph>
         </akn:article>
         <!-- Artikel 3: Geltungszeitregel-->
         <akn:article refersTo="geltungszeitregel"
                      GUID="1123af3c-65db-46d5-b5f0-59c486a6ead7"
                      eId="art-z3"
                      period="#meta-n1_geltzeiten-n1_geltungszeitgr-n1">
            <akn:num GUID="e9da094e-a1a3-4e1c-b4b0-b887fc3fd3e0" eId="art-z3_bezeichnung-n1"> Artikel 3</akn:num>
            <akn:heading GUID="e76050c8-a904-428d-a7c1-f46ccd904469" eId="art-z3_überschrift-n1">Inkrafttreten</akn:heading>
            <akn:paragraph GUID="44b08218-300f-43cc-93a8-9a11e9a13d96" eId="art-z3_abs-z">
               <akn:num GUID="cdd878cb-cdba-48ba-84ae-429507c43e17"
                        eId="art-z3_abs-z_bezeichnung-n1"/>
               <akn:content GUID="83eed131-1c20-426d-b65d-86ddc2907eed"
                            eId="art-z3_abs-z_inhalt-n1">
                  <akn:p GUID="32df9f4f-7d91-406f-835f-34f274b21ccd"
                         eId="art-z3_abs-z_inhalt-n1_text-n1"> Dieses Gesetz tritt <akn:date date="2017-03-16"
                               refersTo="inkrafttreten-datum"
