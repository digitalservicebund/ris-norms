{
  "name": "ris-norms-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "coverage": "TZ=UTC vitest run --coverage",
    "test": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "eslint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --max-warnings 0 .",
    "eslint:fix": "npm run eslint:check -- --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "style:check": "npm run prettier:check && npm run eslint:check",
    "style:fix": "npm run prettier:fix && npm run eslint:fix",
    "test:a11y": "playwright test --project=a11y",
    "test:e2e": "playwright test",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:msedge": "playwright test --project=msedge",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "engines": {
    "node": "20.17.0",
    "npm": "10.8.3"
  },
  "dependencies": {
    "@codemirror/lang-xml": "~6.1.0",
    "@digitalservicebund/ris-ui": "~1.0.0",
    "@sentry/vue": "~8.28.0",
    "@types/lodash.sortby": "~4.7.9",
    "@types/uuid": "~10.0.0",
    "@vueuse/core": "~11.0.0",
    "codemirror": "~6.0.1",
    "dayjs": "~1.11.10",
    "immer": "~10.1.1",
    "lodash.sortby": "~4.7.0",
    "maska": "~3.0.0",
    "primevue": "~4.0.4",
    "uuid": "~10.0.0",
    "vue": "~3.5.0",
    "vue-router": "~4.4.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "~4.10.0",
    "@digitalservice4germany/angie": "~1.2.0",
    "@digitalservice4germany/style-dictionary": "~2.0.0",
    "@iconify-json/ic": "~1.2.0",
    "@playwright/test": "~1.46.0",
    "@sentry/vite-plugin": "^2.20.1",
    "@testing-library/jest-dom": "~6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "~8.1.0",
    "@typescript-eslint/eslint-plugin": "~8.4.0",
    "@typescript-eslint/parser": "~8.4.0",
    "@vitejs/plugin-vue": "~5.1.0",
    "@vitest/coverage-v8": "~2.0.0",
    "@vue/eslint-config-prettier": "~9.0.0",
    "@vue/eslint-config-typescript": "~13.0.0",
    "autoprefixer": "~10.4.19",
    "axe-playwright": "~2.0.1",
    "dotenv": "~16.4.5",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-import": "~2.30.0",
    "eslint-plugin-playwright": "~1.6.0",
    "eslint-plugin-prettier": "~5.2.0",
    "eslint-plugin-testing-library": "~6.3.0",
    "eslint-plugin-vue": "~9.28.0",
    "eslint-plugin-vue-scoped-css": "~2.8.0",
    "eslint-plugin-vuejs-accessibility": "~2.4.0",
    "jsdom": "~25.0.0",
    "license-checker": "~25.0.1",
    "playwright": "~1.46.0",
    "postcss": "~8.4.38",
    "prettier": "~3.3.0",
    "prettier-plugin-tailwindcss": "~0.6.0",
    "tailwindcss": "~3.4.3",
    "typescript": "~5.5.0",
    "unplugin-icons": "~0.19.0",
    "vite": "~5.4.0",
    "vitest": "~2.0.0",
    "vue-tsc": "~2.1.0",
    "xpath": "~0.0.34"
  }
}
