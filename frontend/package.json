{
  "name": "ris-norms-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "coverage": "TZ=UTC vitest run --coverage",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "eslint:check": "eslint --max-warnings 0 .",
    "eslint:check:imports": "eslint --max-warnings 0 --config eslint-imports.config.js .",
    "eslint:fix": "npm run eslint:check -- --fix",
    "eslint:fix:imports": "npm run eslint:check:imports -- --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "style:check": "npm run prettier:check && npm run eslint:check:imports",
    "style:fix": "npm run prettier:fix && npm run eslint:fix:imports",
    "test": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest",
    "test:report": "TZ=UTC vitest run --reporter=html --outputFile=./test-results/unit-tests/report.html",
    "test:a11y": "playwright test --project=a11y",
    "test:e2e": "playwright test",
    "test:browsers": "playwright test --project=chromium --project=firefox",
    "test:smoke": "playwright test --project=smoketest",
    "audit:licences": "license-checker-rseidelsohn --production --excludePrivatePackages --onlyAllow \"$(. ./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv",
    "preinstall": "echo 'Please disable script execution: npm config set ignore-scripts true' && exit 1"
  },
  "engines": {
    "node": "24.11.1",
    "npm": "11.6.3"
  },
  "dependencies": {
    "@codemirror/lang-xml": "~6.1.0",
    "@digitalservicebund/ris-ui": "~3.21.0",
    "@sentry/vue": "~10.34.0",
    "@types/lodash.sortby": "~4.7.9",
    "@types/uuid": "~11.0.0",
    "@vueuse/core": "~14.1.0",
    "codemirror": "~6.0.1",
    "dayjs": "~1.11.13",
    "immer": "~11.1.3",
    "keycloak-js": "~26.2.2",
    "lodash.sortby": "~4.7.0",
    "primevue": "~4.5.4",
    "uuid": "~13.0.0",
    "vue": "~3.5.26",
    "vue-router": "~4.6.4",
    "zod": "~4.3.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "~4.11.0",
    "@eslint/compat": "~2.0.1",
    "@eslint/js": "~9.39.2",
    "@iconify-json/ic": "~1.2.2",
    "@playwright/test": "~1.57.0",
    "@sentry/vite-plugin": "~4.6.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "~6.9.0",
    "@testing-library/user-event": "~14.6.1",
    "@testing-library/vue": "~8.1.0",
    "@types/node": "~25.0.9",
    "@typescript-eslint/eslint-plugin": "~8.53.0",
    "@typescript-eslint/parser": "~8.53.0",
    "@vitejs/plugin-vue": "~6.0.3",
    "@vitest/coverage-istanbul": "~4.0.17",
    "@vitest/coverage-v8": "~4.0.17",
    "@vitest/eslint-plugin": "~1.6.6",
    "@vitest/ui": "~4.0.0",
    "@vue/eslint-config-prettier": "~10.2.0",
    "@vue/eslint-config-typescript": "~14.6.0",
    "dotenv": "~17.2.0",
    "eslint": "~9.39.2",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-playwright": "~2.5.0",
    "eslint-plugin-testing-library": "~7.15.4",
    "eslint-plugin-vue": "~10.6.2",
    "eslint-plugin-vuejs-accessibility": "~2.4.1",
    "globals": "~17.0.0",
    "jsdom": "~27.4.0",
    "license-checker-rseidelsohn": "~4.4.2",
    "playwright": "~1.57.0",
    "postcss": "~8.5.3",
    "prettier": "~3.8.0",
    "prettier-plugin-tailwindcss": "~0.7.2",
    "tailwindcss": "~4.1.0",
    "typescript": "~5.9.0",
    "unplugin-icons": "~22.5.0",
    "vite": "~7.3.1",
    "vitest": "~4.0.0",
    "vue-tsc": "~3.2.2",
    "xpath": "~0.0.34"
  },
  "overrides": {
    "nwsapi": "2.2.20"
  }
}
