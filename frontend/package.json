{
  "name": "ris-norms-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "coverage": "TZ=UTC vitest run --coverage",
    "test": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest",
    "preview": "vite preview",
    "eslint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "eslint:fix": "npm run eslint:check -- --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "style:check": "npm run prettier:check && npm run eslint:check",
    "style:fix": "npm run prettier:fix && npm run eslint:fix",
    "test:a11y": "playwright test --project=a11y",
    "test:e2e": "playwright test",
    "histoire": "histoire dev",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./parseAllowedLicences.sh)\" --csv --out ./licence-report.csv"
  },
  "engines": {
    "node": "20.10.0",
    "npm": "10.2.3"
  },
  "dependencies": {
    "vue": "~3.4.15",
    "vue-router": "~4.2.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "~4.8.4",
    "@digitalservice4germany/angie": "~1.1.1",
    "@digitalservice4germany/style-dictionary": "~2.0.0",
    "@histoire/plugin-vue": "~0.17.9",
    "@iconify-json/ic": "~1.1.17",
    "@playwright/test": "~1.41.1",
    "@testing-library/jest-dom": "~6.4.0",
    "@testing-library/vue": "~8.0.1",
    "@typescript-eslint/eslint-plugin": "~6.20.0",
    "@typescript-eslint/parser": "~6.19.1",
    "@vitejs/plugin-vue": "~5.0.3",
    "@vitest/coverage-v8": "~1.2.1",
    "@vue/eslint-config-prettier": "~9.0.0",
    "@vue/eslint-config-typescript": "~12.0.0",
    "autoprefixer": "~10.4.17",
    "axe-playwright": "~2.0.1",
    "eslint": "~8.56.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-import": "~2.29.1",
    "eslint-plugin-playwright": "~0.22.1",
    "eslint-plugin-prettier": "~5.1.3",
    "eslint-plugin-testing-library": "~6.2.0",
    "eslint-plugin-vue": "~9.20.1",
    "eslint-plugin-vue-scoped-css": "~2.7.2",
    "eslint-plugin-vuejs-accessibility": "~2.2.1",
    "histoire": "~0.17.9",
    "jsdom": "~24.0.0",
    "license-checker": "~25.0.1",
    "playwright": "~1.41.1",
    "postcss": "~8.4.33",
    "prettier": "~3.2.4",
    "prettier-plugin-tailwindcss": "~0.5.11",
    "tailwindcss": "~3.4.1",
    "typescript": "~5.3.3",
    "unplugin-icons": "~0.18.2",
    "vite": "~5.0.12",
    "vitest": "~1.2.1",
    "vue-tsc": "~1.8.27"
  }
}
